<script>
  $(function () {
    const updateHash = () => {
      const activeTechnologyBtn = [
        ...document.querySelectorAll(
          '#men-met-guard-20220520 button[data-technology][data-active="true"]'
        ),
      ];

      const activeTechnology = activeTechnologyBtn.map((btn) => btn.dataset.technology);

      if (activeTechnology == 'All') techParam = 'start=0&sz=12'
      else if (activeTechnology == 'Contour') techParam = 'prefn1=technologyIcons&prefv1=Contour Met'
      else if (activeTechnology == 'Terrapin') techParam = 'prefn1=technologyIcons&prefv1=Terrapin Met'
      else if (activeTechnology == 'XRD') techParam = 'prefn1=technologyIcons&prefv1=XRD Protection'
      else techParam = 'start=0&sz=12';

      const hash = '#' + techParam;

      window.location.hash = hash;
    };

    const handleClick = (e) => {
      // Turn all stuff inactive
      const filterStuff = document.querySelectorAll('#men-met-guard-20220520 [data-technology]');
      filterStuff.forEach((thing) => thing.dataset.active = 'false');
      // Set current Tech Stuff active
      const currentTech = e.target.dataset.technology;
      const filterString = '#men-met-guard-20220520 [data-technology="' +  currentTech + '"]';
      const filterTech = document.querySelectorAll(filterString);
      filterTech.forEach((thing) => thing.dataset.active = 'true');

      updateHash();
    };

    // Add handleClick to the buttons
    const filterBtns = document.querySelectorAll('#men-met-guard-20220520 button');
    filterBtns.forEach((btn) => btn.addEventListener('click', handleClick));
  });
</script>


 
	
		
