    <script>
  // Comprehensive bot/crawler user agent detection
  const UserAgentDetectionPattern = /bot|googlebot|google|bing|mediapartners-google|google-inspectiontool|googleother|google-storebot|googleother-video|google-extended|bingbot|bingpreview|adidxbot|slurp|yandexbot|yandeximages|sogou|duckduckbot|duckassistbot|applebot|facebookexternalhit|facebot|twitterbot|linkedinbot|pinterestbot|bytespider|snapchat|snapchatadsbot|snapbot|amazonbot/i;

  function isBot(userAgent) {
    return UserAgentDetectionPattern.test(userAgent);
  }

  if (!isBot(navigator.userAgent)) {
    function getCookie(cookieName) {
      let cookie = {};
      document.cookie.split(';').forEach(function(el) {
        let [key,value] = el.split('=');
        cookie[key.trim()] = value;
      });
      return cookie[cookieName];
    }

    function deleteCookie(cookieName) {
      document.cookie = cookieName + '=; max-age=0; path=/';
    }

    function getSlashPos(str, i) {
      return str.split('/', i).join('/').length;
    }

    (function () {
      // Read Vanity page url from head element
      let vanityPath = document.querySelector('meta[name="vanityPagePath"]');
      let vanityCookiePath = getCookie("MMVanityCookie");
      if (vanityCookiePath) deleteCookie('MMVanityCookie');
      let locationCookie = getCookie("MMLocationCookie");
      let target;
      if (locationCookie && vanityCookiePath) {
        let pathIndex = getSlashPos(vanityCookiePath, 3);
        let pathWithoutLocation = vanityCookiePath.substring(pathIndex, vanityCookiePath.length);
        target = '/' + locationCookie + pathWithoutLocation;
      } else if (vanityPath && vanityPath.content) {
        let redirectPath = vanityPath.content;
        // Assuming path has format /content/mercury-marine/{country}/{language}/
        let index = locationCookie ? 5 : 3;
        let pathIndex = getSlashPos(redirectPath, index);
        let prefix = locationCookie ? '/' : '';
        target = prefix + (locationCookie || '') + redirectPath.substring(pathIndex, redirectPath.length);
      }
      if (target && target.length) window.location.replace(window.location.origin + target);
    })();
  }
</script>


