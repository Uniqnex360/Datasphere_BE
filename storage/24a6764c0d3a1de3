<script>
document.addEventListener('DOMContentLoaded', () => {
  const tabComponents = document.querySelectorAll('.c-tab');

  tabComponents.forEach(tabComponent => {
    const tabs = tabComponent.querySelectorAll('.c-tab__link');
    const tabContents = tabComponent.querySelectorAll('.c-tab__content');

    tabs.forEach(tab => {
      tab.addEventListener('click', (e) => {
        if (tab.getAttribute('href') === '#') {
          e.preventDefault();
        }

        const tabName = tab.dataset.tab;
        const selectedContent = tabComponent.querySelector(`[data-content="${tabName}"]`);
        tabs.forEach(tab => {
          tab.classList.remove("active");
        });
        tabContents.forEach(content => {
          content.classList.remove("active");
        });
        if (selectedContent) {
          tab.classList.add("active");
          selectedContent.classList.add("active");
        }
      });
    });
  });
});

</script>

