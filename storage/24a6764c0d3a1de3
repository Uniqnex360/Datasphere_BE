<script>
window.addEventListener("DOMContentLoaded", function(event) {
	var form = document.querySelector('.c-footer__primary-container > div > div.col-xs-12 > form');
	
	if (form != null && form != undefined) {
		form.addEventListener('submit', function(event) {
			event.preventDefault(); 
		});
	}


document.getElementById("subscribe-email").addEventListener("keyup", function(event) {
// Number 13 is the "Enter" key on the keyboard
if (event.keyCode == 13)
{
var button = document.getElementById("newsletter_subscribe");
    button.classList.add('c-footer__tick');
   button.value = '✓';
   button.setAttribute("disabled", "");
}

})

})



</script>
<script>
const checkExists = setInterval(function () {
if (document.querySelectorAll(".acsb-language")[2]){
    let English = 1;
	let languageToChange = document.getElementsByClassName("acsb-language")[English];	
	let imageUrl = "https://www.lowrance.com/assets/img/flags/flag-gb.svg";
	let activeLanguage = document.getElementsByClassName("acsb-language")[0].firstElementChild;
	let selectedLanguageText = languageToChange.children[1].textContent;
	let activeLanguageText = document.getElementsByClassName("acsb-language-text")[0].textContent;
	languageToChange.firstElementChild.innerHTML=`<img src=${imageUrl} alt="${selectedLanguageText}">`
	if(selectedLanguageText === activeLanguageText){
		activeLanguage.innerHTML =`<img src=${imageUrl} alt="${selectedLanguageText}">`
	}	
	clearInterval(checkExists);
}else{}
},100);
</script>
