<script>
jQuery( document ).on( "click", ".expand-btn", function() {
  jQuery('.expand-img').css('height', 'auto');
  jQuery(this).find('button').html('Less Then <i class="vc_btn3-icon fa fa-angle-up"></i>');
  jQuery(this).addClass('expnded');
  jQuery('html, body').animate({
        scrollTop: jQuery(".expand-img").offset().top - 300
    }, 500);
});
jQuery( document ).on( "click", ".expand-btn.expnded", function() {
  jQuery('.expand-img').css('height', '290px');
  jQuery(this).find('button').html('Read More <i class="vc_btn3-icon fa fa-angle-down"></i>');
  jQuery(this).removeClass('expnded');
  jQuery('html, body').animate({
        scrollTop: jQuery(".expand-img").offset().top - 100
    }, 500);

jQuery("body").html(
jQuery("body").html()
    .replace(/((?!<sup>\s*))&reg;((?!\s*<\/sup>))/gi, '<sup>&reg;</sup>') // wrap &reg; if not wrapped yet
    .replace(/((?!<sup>\s*))((?!\s*<\/sup>))/gi, '<sup>&reg;</sup>') // wrap  if not wrapped yet
);

});
</script>
<script>
jQuery(window).load(function () {
setTimeout(function () {
jQuery('div[u="loading"]').css('display', 'none');
jQuery('div[u="slides"]').css('opacity', '1');
}, 600);

});
jQuery(document).ready(function($){
	jQuery('a[href="https://www.seymourduncan.com/resources/pickup"], a[href="https://www.seymourduncan.com/resources/pedals"], a[href="https://www.seymourduncan.com/resources/support"]').attr('href','javascript:;');
	jQuery('.wpb_wrapper > .sidebar-cat').closest('.vc_column-inner').addClass('category-filters-wrapper');
	jQuery(window).on('resize', function(){ mobileFilterDropDown(); });
	mobileFilterDropDown();

	if( jQuery(window).width() < 768 ){
		jQuery('.product-top').parent().addClass('filter-row-list');
		jQuery('.filter-row-list').insertBefore('.category-filters-wrapper');
	};
	jQuery('a.button.product_type_simple').addClass('add_to_cart_button');
});
function mobileFilterDropDown(){
	if( jQuery(window).width() < 768 ){
		if( jQuery('.category-filters-wrapper > .category-filters').length == 0 ){
			jQuery('.category-filters-wrapper > .wpb_wrapper').before('<div class="category-filters filters-close" onclick="mobileFilterToggle(this)"><h4 class="category-filters-head">FILTERS</h4><span class="category-filters-option">+</span></div>');
			//jQuery('.category-filters-wrapper > .wpb_wrapper').slideUp();
		}
	} else {
		jQuery('.category-filters-wrapper > .category-filters').remove();
		jQuery('.category-filters-wrapper > .wpb_wrapper').slideDown();
	}
}
function mobileFilterToggle(obj){
	if( jQuery(obj).hasClass('filters-close') ) {
		jQuery(obj).toggleClass('filters-close filters-open');
		jQuery(obj).find('span').text('_');
		jQuery('.category-filters-wrapper > .wpb_wrapper').slideDown();
	} else {
		jQuery(obj).toggleClass('filters-open filters-close');
		jQuery(obj).find('span').text('+');
		jQuery('.category-filters-wrapper > .wpb_wrapper').slideUp();
	}
}
</script>
<style>
.category-filters { cursor: pointer;position: relative;margin: 10px 0px;padding-bottom: 25px; }
.category-filters .category-filters-head { font-size: 16px;float: left; }
.category-filters .category-filters-option {width: 20px;text-align: center;font-size: 30px;font-weight: bold;position: absolute;right: 0px;top: -13px; }
.category-filters.filters-open .category-filters-option { top: -25px; }
</style>

<a id="back-to-top-button"></a>
<script>
var btn = jQuery('#back-to-top-button');

jQuery(window).scroll(function() {
  if (jQuery(window).scrollTop() > 300) {
    btn.addClass('show');
  } else {
    btn.removeClass('show');
  }
});

btn.on('click', function(e) {
  e.preventDefault();
  jQuery('html, body').animate({scrollTop:0}, '300');
});
</script>
</body>
</html>
