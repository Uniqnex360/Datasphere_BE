function checkMegaMenu() {
    if(jQuery('ul.mega-sub-menu').length) {
        jQuery("ul.mega-sub-menu .mega-menu-column.mega-first .mega-menu-item:nth-child(1)").addClass('active-tab-menu');
        jQuery("ul.mega-sub-menu .mega-menu-column.mega-first .mega-menu-item:nth-child(2)").addClass('active-tab-menu-box');

        jQuery(".mega-sub-menu > .mega-menu-item").on('click', function(){
            jQuery(this).closest('.mega-menu-row').find('.mega-sub-menu > .mega-menu-item').removeClass('active-tab-menu');
            jQuery(this).closest('.mega-menu-row').find('.mega-sub-menu > .mega-menu-item + .mega-menu-item').removeClass('active-tab-menu-box');

            jQuery(this).addClass('active-tab-menu');
            jQuery(this).next('li').addClass('active-tab-menu-box');

            if(jQuery(this).parent().parent().hasClass('first')){

            }
            else{

            }
        });
    } else {
        setTimeout(function() {
            checkMegaMenu()
        }, 100)
    }
}

jQuery(function() {
    setTimeout(function() {
        checkMegaMenu()
    }, 100)
});
