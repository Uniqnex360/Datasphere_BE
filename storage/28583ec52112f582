<script>
  (function($){
    var $header = $('.headerBg'),
      $headerParent = $header.parent(),
      originalHeaderParentPadding = $headerParent.css('padding-top'),
      stickyNavTargetScroll = 500;
    $(window).on('scroll', function(){
      if($(window).scrollTop() > stickyNavTargetScroll){
        var headerHeight = $header.outerHeight();
        $headerParent.css('padding-top', headerHeight);
        $header.addClass('stickyNav');
      } else {
        $headerParent.css('padding-top', originalHeaderParentPadding);
        $header.removeClass('stickyNav');
      }
    });
  })(jQuery);
</script>
<script>
jQuery(document).ready(function(){
jQuery(document).on('click', 'button.is-active', function() {
  jQuery(this).removeClass('is-active');
});

    jQuery(".filter-button").click(function(){
        var value = jQuery(this).attr('data-filter');

        if(value == "electric")
        {
            //jQuery('.filter').removeClass('hidden');
            jQuery(".filter").not('.'+value).hide('3000');
            jQuery('.filter').filter('.'+value).show('3000');
        }
        else
        {
//            jQuery('.filter[filter-item="'+value+'"]').removeClass('hidden');
//            jQuery(".filter").not('.filter[filter-item="'+value+'"]').addClass('hidden');
            jQuery(".filter").not('.'+value).hide('3000');
            jQuery('.filter').filter('.'+value).show('3000');
			jQuery(".filter-button").removeClass("btnActive");
        }
        jQuery(this).addClass('btnActive').siblings(".filter-button").removeClass('btnActive');
    });

if (jQuery(".filter-button").removeClass("active")) {
jQuery(this).removeClass("active");
}
jQuery(this).addClass("active");

