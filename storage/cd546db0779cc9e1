</div><div id="shopify-section-template--14231370334251__product_recently_viewed_aEfJEC" class="shopify-section section-product-recently-viewed"><div id="product-recently-viewed" class="product-recently-viewed"
  data-section-id="template--14231370334251__product_recently_viewed_aEfJEC"
  data-components="accordion,modal"
  data-cc-animate>
  <div class="container fully-spaced-row">
    <h4 class="align-center hometitle">Recently viewed</h4>
    <div class="collection-listing related-collection">
      <div class="product-list product-list--per-row-4 product-list--image-shape-natural" id="recently-viewed-products-container"><!-- Products will be inserted here by JavaScript -->
      </div>
    </div>
  </div>
</div>

<script>
  // Store current product in recently viewed (if we're on a product page)
  document.addEventListener('DOMContentLoaded', function() {
    const currentProductId = 1745474420779;
    const currentProductHandle = 'black-3-8-x-15-premium-double-braid-dock-line';
    
    // Only proceed if we're on a product page
    if (currentProductId === 0) return;
    
    // Get existing recently viewed products
    let recentlyViewed = JSON.parse(localStorage.getItem('recentlyViewedProducts') || '[]');
    
    // Remove the current product if it's already in the list
    recentlyViewed = recentlyViewed.filter(item => item.id !== currentProductId);
    
    // Add the current product to the end of the list
    recentlyViewed.push({
      id: currentProductId,
      handle: currentProductHandle,
      timestamp: new Date().getTime()
    });
    
    // Limit to last 12 products
    if (recentlyViewed.length > 12) {
      recentlyViewed = recentlyViewed.slice(-12);
    }
    
    // Save back to localStorage
    localStorage.setItem('recentlyViewedProducts', JSON.stringify(recentlyViewed));
    
    // Render recently viewed products (exclude current product)
    renderRecentlyViewedProducts(currentProductId);
  });

  // Function to render recently viewed products
  function renderRecentlyViewedProducts(currentProductId) {
    // Get recently viewed products from localStorage
    const recentlyViewed = JSON.parse(localStorage.getItem('recentlyViewedProducts') || '[]');
    
    // Filter out current product and sort by recent view
    const filtered = recentlyViewed
      .filter(item => item.id !== currentProductId)
      .sort((a, b) => b.timestamp - a.timestamp);
    
    // If no products to show, hide the section
    if (filtered.length === 0) {
      document.getElementById('product-recently-viewed').style.display = 'none';
      return;
    }
    
    // Take only up to max products to display
    const maxProducts = 4;
    const productsToFetch = filtered.slice(0, maxProducts);
    
    // Container for products
    const container = document.getElementById('recently-viewed-products-container');
    
    // Track fetched products to avoid duplicates
    const fetchedProductIds = new Set();
    
    // Fetch each product by handle
    Promise.all(productsToFetch.map(item => {
      if (fetchedProductIds.has(item.id)) return null;
      fetchedProductIds.add(item.id);
      
      return fetch(`/products/${item.handle}?view=product-card-json`)
        .then(response => response.json())
        .catch(() => null);
    }))
    .then(products => {
      // Filter out nulls and empty responses
      const validProducts = products.filter(p => p !== null);
      
      if (validProducts.length === 0) {
        document.getElementById('product-recently-viewed').style.display = 'none';
        return;
      }
      
      // Clear loading state
      container.innerHTML = '';
      
      // Render each product
      validProducts.forEach(product => {
        const chosen_aspect_ratio = 1;
        const productHtml = renderProductCard(product, chosen_aspect_ratio);
        const tempDiv = document.createElement('div');
        tempDiv.innerHTML = productHtml.trim();
        container.appendChild(tempDiv.firstChild);
      });
    })
    .catch(error => {
      console.error('Error fetching recently viewed products:', error);
      document.getElementById('product-recently-viewed').style.display = 'none';
    });
  }

  // Function to render a product card with data
  function renderProductCard(product, aspect_ratio) {
    const productUrl = `/products/${product.handle}`;
    return `
      <div class="product-block" data-product-id="${product.id}">
        <div class="block-inner box-border">
          <div class="block-inner-inner">
           
              <a href="${productUrl}" class="product-link">
                <div class="image-label-wrap">
                  <div class="image-label-wrap-inner">
                    <div class="product-block__image">
                      <div class="rimage-outer-wrapper">
                        <div class="rimage-wrapper" style="padding-top:${1 / aspect_ratio * 100}%;">
                          <img src="${product.featured_image}" alt="${product.title}" class="rimage__image">
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </a>
            <div class="product-info">
              <div class="product-price">
                <div class="inner">
                  <div class="innerer">
                    <a class="product-link" href="${productUrl}">
                      <div class="product-block__title">${product.title}</div>
                      <div class="product-price">${product.price_html}</div>    
                    </a>    
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    `;
  }
</script>

</div><div id="shopify-section-template--14231370334251__recommendations" class="shopify-section section-product-recommendations"><div class="product-recommendations"
    data-section-id="template--14231370334251__recommendations"
