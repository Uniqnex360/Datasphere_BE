            const jsonResponse = await response.json();
            console.log("GraphQL API Response:", jsonResponse);

            if (!jsonResponse.data.site.product) {
                throw new Error("No product data returned from API");
            }

            // Extract custom fields
            const customFields = jsonResponse.data.site.product.customFields.edges.map(edge => edge.node);
            console.log("Custom Fields:", customFields);
            
            let mapPrice = null;
            customFields.forEach(field => {
                if (field.name === "map_pricing_eagle") {
                    mapPrice = parseFloat(field.value);
                }
            });
            console.log("MAP Price:", mapPrice);
            
