<!-- Inline JavaScript to fix $0.00 pricing -->
<script>
  // Function to update price display.
  function fixZeroPrice(context) {
    var container = context || document;
    container.querySelectorAll(".kuPrice .kuSalePrice").forEach(function(el) {
      var text = el.textContent.trim();
      if (text === "$0.00" || text === "$ 0.00") {
        el.textContent = "See price at checkout";
      }
    });
  }

  // Run the function on initial load.
  document.addEventListener("DOMContentLoaded", function() {
    fixZeroPrice();
  });

  // Set up a MutationObserver to catch dynamically loaded content.
  (function() {
    var observer = new MutationObserver(function(mutations) {
      mutations.forEach(function(mutation) {
        mutation.addedNodes.forEach(function(node) {
          if (node.nodeType === 1) { // element node
            fixZeroPrice(node);
          }
        });
      });
    });

    // Observe the document body or a more specific container if available.
    observer.observe(document.body, { childList: true, subtree: true });
  })();
</script>

