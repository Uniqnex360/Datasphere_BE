            let productName = pathname.replace(/^\/|\/$/g, '').replace(/-/g, ' '); // Removes leading/trailing slashes and replaces "-" with spaces
            console.log(document.querySelectorAll('script[type="application/ld+json"]').length);

            console.log("Extracted Product Name:", productName);
            console.log("Product Inventory Level in Dom:", "");
            
            let stockStatus = window.BCData?.product_attributes?.instock;
            let stockElement = document.querySelector(".cnc__shipping-availability"); //data-product-stock, .cnc__shipping-availability
            
            let imageElement = document.querySelector('meta[property="og:image"]');
            let productImage = imageElement ? imageElement.content : "";
            console.log("Extracted Image URL:", productImage);
            
            let stockLevel = stockElement ? stockElement.textContent.trim() : "Unknown";
            let availability = stockStatus ? "InStock" : "OutOfStock";
            //let availability = stockLevel !== "Unknown" && parseInt(stockLevel) > 0 ? "InStock" : "OutOfStock";

            console.log("Extracted Stock Level:", stockStatus);
            console.log("Stock Level with stockElement: ", stockElement);
             console.log("Stock Level with stockLevel: ", stockLevel);
            console.log("Extracted Stock availability: ", availability);
            console.log("Product category: ", "Lawn &amp; Garden/Lawn Growth");
            
            //console.log("Product Image Check", imageElement.value);
            
             //"priceCurrency": "",

            // Modify JSON-LD schema dynamically
            let jsonLDElement = document.querySelector('script[type="application/ld+json"]');
            let jsonText = jsonLDElement.textContent
                    .replace(/[\u0000-\u001F\u007F]/g, '')  // Remove bad control characters
                    .replace(/\\"/g, '"'); // Fix escaped quotes

                let jsonLDData = JSON.parse(jsonText);

                // Assign extracted name while ensuring proper escaping
                jsonLDData.name = productName;
                jsonLDData.offers.availability = `https://schema.org/${availability}`; // Update stock status
                jsonLDData.image = [productImage];

                // Update JSON-LD script with formatted JSON
                jsonLDElement.textContent = JSON.stringify(jsonLDData, null, 2);
                
        } catch (error) {
            console.error("Error extracting or updating product name:", error);
        }
