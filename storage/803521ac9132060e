    document.addEventListener("DOMContentLoaded", async function () { //***DONT FORGET TO TAKE OUT ASYNC PROBABLY, careful: might mess with web crawler. 
       
            // Get the pathname and extract the product name
           try {
            // Get the pathname and extract the product name
