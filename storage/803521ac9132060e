        <script>

            document.addEventListener("DOMContentLoaded",() => {
                const meta = document.querySelector('meta[property="product:price:amount"]');
                if (meta) {meta.remove();}

                const prop = document.querySelector('meta[property="product:price:amount"]');
                if (prop) {meta.remove();}

                const pricing = document.querySelectorAll('.price--withoutTax')
                if (pricing){
                    pricing.forEach(item => {
                        const span = item.querySelector('span')
                        const span_price = item.querySelector('.map-pricing-eagle-hidden')
                        
                        if(span_price && span ){
                            const span_real = span_price.textContent || span_price.innerText
                            const maping_price = span.textContent || span.innerText
                           

                            const price_without_tax = parseFloat(maping_price.replace('$',"").replace(',',""))
                            const price_with_tax = parseFloat (span_real) 
                            

                            if(price_without_tax >= price_with_tax ){
                                setTimeout(() => {
                                    const remove_map = item.parentElement
                                    
                                    if(remove_map.classList.contains('MAP-checkout')){
                                        remove_map.classList.remove('MAP-checkout')
                                        remove_map.classList.add('original-checkout') 
                                        
                                    }
                                    
                                }, "1000");

                            }
                            else{
                                setTimeout(() => {
                                    const remove_map = item.parentElement
                                 
                                    if(remove_map.classList.contains('original-checkout')){
                                        remove_map.classList.add('MAP-checkout')
                                        remove_map.classList.remove('original-checkout')
                                    }
                                    item.remove()
                                    
                                }, "900");
                            }
                           
                           
                        }

                        
                    })
                }
        
   
                const no_tax = document.querySelector('[data-product-non-sale-price-without-tax]')
                const tax = document.querySelector('[data-product-price-without-tax]')
                

                if (no_tax && tax){
                    
                    let no_tax_price = no_tax.textContent || no_tax.innerText
                    let tax_price = tax.textContent || tax.innerText

                    no_tax_price = parseFloat(no_tax_price.replace('$',"").replace(',',""))
                    //tax_price = parseFloat(tax_price.replace('$',"").replace(',',"")) 

                    if( no_tax_price > tax_price ){ 
                        tax.remove()
                                
                    }
                    
                }  


        
        }) 
    </script>

        

