    document.addEventListener("DOMContentLoaded", () => {
      const observer = new MutationObserver((mutationsList) => {
        console.log("Mutation detected!");
    
        mutationsList.forEach((mutation) => {
          if (mutation.type === "childList" || mutation.type === "subtree") {
            const stockElementsTwo = document.querySelectorAll('.cnc__shipping-availability');
    
            let inStoreNoStock = false;
            let onlineNoStock = false;
    
            if (stockElementsTwo.length > 0) {
              console.log("Stock elements detected:", stockElementsTwo);
    
              stockElementsTwo.forEach((element) => {
                const stockTextThree = element.textContent.trim().toLowerCase();
                if (stockTextThree.includes("no stocks available for pickup")) {
                  console.log("No stock for pickup detected:", stockTextThree);
                  inStoreNoStock = true;
                }
                if (stockTextThree.includes("unavailable in-store")) {
                  console.log("Unavailable in-store detected:", stockTextThree);
                  onlineNoStock = true;
                }
              });
    
              let jsonLDElementThree = document.querySelector('script[type="application/ld+json"]');
              if (jsonLDElementThree) {
                let jsonTextThree = jsonLDElementThree.textContent
                  .replace(/[\u0000-\u001F\u007F]/g, '') // Remove bad control characters
                  .replace(/\\"/g, '"'); // Fix escaped quotes
    
                let jsonLDDataThree = JSON.parse(jsonTextThree);
    
                if (onlineNoStock && inStoreNoStock) {
                  let availabilityNew = "OutOfStock";
                  jsonLDDataThree.offers.availability = `https://schema.org/${availabilityNew}`;
                  console.log("Schema updated: OutOfStock");
                } else {
                  let availabilityNew = "InStock";
                  jsonLDDataThree.offers.availability = `https://schema.org/${availabilityNew}`;
                  console.log("Schema updated: InStock");
                }
    
                // Replace the JSON-LD script dynamically
              //  const newSchema = document.createElement('script');
            //    newSchema.type = "application/ld+json";
                jsonLDElementThree.textContent = JSON.stringify(jsonLDDataThree, null, 2);
              //  jsonLDElementThree.parentNode.replaceChild(newSchema, jsonLDElementThree);
    
                console.log("Schema successfully updated and replaced in the DOM");
              }
    
              observer.disconnect(); // Stop observing after the update
            }
          }
        });
      });
    
      // Observe the entire body since elements might be injected dynamically
      observer.observe(document.body, { childList: true, subtree: true });
    });
    
        
</script>



