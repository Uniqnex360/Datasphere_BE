<script>

/*Carousel on Homepage Script*/

jQuery(document).ready(function(){
jQuery('.swiper-pagination-bullet').each(function(){

//Create span with class for labels
let c = document.createElement('span');
c.classList.add('dotLabel');

//Create node from slides Aria Label
let n = document.createTextNode(jQuery(this).attr('aria-label'));

//Add to the created span
c.appendChild(n);
    jQuery(this).before(c.outerHTML);

});

//Wrap Span and Button
jQuery(".swiper-pagination-bullets > span").each(function () {
  jQuery(this).next("button").andSelf().wrapAll('<div class="slideWrap"/>');
});

//Have span/button switch positions
jQuery('.slideWrap').each(function(){
    jQuery(this).children(":first-child").before(jQuery(this).children(":last-child"));
});

//Checks active slide and styles the label
if(jQuery('button').hasClass('.swiper-pagination-bullet-active')){
    jQuery('button .swiper-pagination-bullet-active').next('span').css('color', '#24433D');
}else{
    jQuery('.dotLabel').css('color', '#FFFFFF');
}

// if(jQuery("button").hasClass('.swiper-pagination-bullet')){
//   jQuery(this).next('.dotLabel').toggleClass('.swiper-pagination-bullet-active');
// }

//Div for under the label
let sbox = document.createElement('div');
sbox.classList.add('slidebox');
 jQuery('.slideWrap').append(sbox);
}
);


</script>	<noscript><style>.woocommerce-product-gallery{ opacity: 1 !important; }</style></noscript>
	<style>.recentcomments a{display:inline !important;padding:0 !important;margin:0 !important;}</style>
