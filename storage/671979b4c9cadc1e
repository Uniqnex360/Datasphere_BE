    <script>
    jQuery(function($) {
        var p = 'p.price'
            q = $(p).html()
			base_title = '.product_title.entry-title'
		    variant_wrapper = '.single_variation_wrap'
		    variant_title = '.woocommerce-variation-description';

        $('form.cart').on('show_variation', function( event, data ) {
            if ( data.price_html ) {
                $(p).html(data.price_html);
				var base_variant_title_selector = `${base_title} ${variant_title}`;
				var wrapped_variant_selector = `${variant_wrapper} ${variant_title}`;
				if ($(base_variant_title_selector).length == 0) {
					$(base_title).append($(wrapped_variant_selector));
				} else {
					$(base_variant_title_selector).html($(wrapped_variant_selector).html());
				}
            }
        }).on('hide_variation', function( event ) {
            $(p).html(q);
        });
    });
    </script>
    
