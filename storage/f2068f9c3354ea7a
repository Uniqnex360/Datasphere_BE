        techFolderOmnis = 'https://techtoolsupply.secure.omnis.com/VR/';
        techFolder = 'https://www.techtoolsupply.info/VR/';
        techFolderBackup = '/v/vspfiles/VR/';
        verifyImage = 'https://www.techtoolsupply.info/urlTest.gif?' + new Date().getTime();

        function backupSource() {
            manualFolder = manualFolderBackup;
            techFolder = techFolderBackup;
        }
    </script>
    <img style="display:none;" id='urlChecker' src="https://www.techtoolsupply.info/urlTest.gif"
        onerror="backupSource()" />
