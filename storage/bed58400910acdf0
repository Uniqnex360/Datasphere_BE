function setSessionConsent(consent) {
    var consentUrl = urls.consentTrackingSetSession + '?consent=' + consent;
    $.ajax({
        type: 'GET',
        url: consentUrl,
        success: function() {
            // nothing to do
           console.log('Cookie Preference saved');
        }
    });
}
