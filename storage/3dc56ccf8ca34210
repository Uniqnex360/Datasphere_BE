  const playVideos = () => {
    document.querySelectorAll("video[should-autoplay]").forEach(video => {
      video.play();
    });
  };

  setTimeout(playVideos, 2000);

  class CampfireDailyBadge extends HTMLElement {
    constructor() {
      super();

      this.deals = {
        "bobcat-45-backpacking-quilt": {
          date: "2023-11-15T00:00:00.000-06:00",
          expire: "2023-11-16T13:00:00.000-06:00",
          price: 199.95,
          salePrice: 124.95
        },
        "roo-single-python-10-bundle": {
          date: "2023-11-15T00:00:00.000-06:00",
          expire: "2023-11-16T13:00:00.000-06:00",
          price: 104.90,
          salePrice: 52.45
        },
        "sunda2-hammock-tent": {
          date: "2023-11-15T00:00:00.000-06:00",
          expire: "2023-11-16T13:00:00.000-06:00",
          price: 419.95,
          salePrice: 269.95
        },
        "mantis-hammock-tent": {
          date: "2023-11-15T00:00:00.000-06:00",
          expire: "2023-11-16T13:00:00.000-06:00",
          price: 249.95,
          salePrice: 149.95,
          variantId: 40009549152310
        },
        "firebelly-30-backpacking-quilt": {
          date: "2023-11-14T00:00:00.000-06:00",
          expire: "2023-11-16T13:00:00.000-06:00",
          price: 299.95,
          salePrice: 199.95
        }
      };

      this.counter = null;
      this.endsIn = 0;

      document.body.addEventListener("variantChange", this.onVariantChange.bind(this));
    }

    connectedCallback() {
      this.onVariantChange({
        detail: {
          variantId: 40009549447222
        }
      });
    }

    onVariantChange(e) {
      const USDollar = new Intl.NumberFormat("en-US", {
        style: "currency",
        currency: "USD"
      });

      const start = new Date(this.deals["roo-double-camping-hammock"].date);
      const end = new Date(this.deals["roo-double-camping-hammock"].expire);

      const startMillis = start.getTime() + end.getTimezoneOffset() * 60000;
      const endMillis = end.getTime() + end.getTimezoneOffset() * 60000;
      const d = new Date();
      const millis = d.getTime() + end.getTimezoneOffset() * 60000;

      if (millis > startMillis && millis <= endMillis) {
        this.classList.remove("hidden");
        document.querySelector(".product-summary .badge--sale").classList.add("!hidden");

        if (this.deals["roo-double-camping-hammock"].variantId && e.detail.variantId !== this.deals["roo-double-camping-hammock"].variantId) {
          return;
        }
  
        if (!this.deals["roo-double-camping-hammock"].price || !this.deals["roo-double-camping-hammock"].salePrice) {
          document.querySelector(".product-summary .badge--sale").classList.add("!hidden");
          return;
        }

        this.endsIn = endMillis - millis + 1000;
        this.startCounter();

        document.querySelector(".product-summary .badge--sale").classList.remove("!hidden");
        document.querySelector(".product-summary s.price-item--regular").textContent = USDollar.format(this.deals["roo-double-camping-hammock"].price);
        document.querySelector(".product-summary .price-item--sale").textContent = USDollar.format(this.deals["roo-double-camping-hammock"].salePrice);
        document.querySelector(".product-summary .price").classList.add("price--on-sale");
      }
    }

    startCounter() {
      clearInterval(this.counter);
      this.counter = setInterval(this.startTimer.bind(this), 1000);
    }

    startTimer() {
      this.endsIn -= 1000;

      if (this.endsIn < 1000) {
        window.location.reload();
      }

      const _second = 1000;
      const _minute = _second * 60;
      const _hour = _minute * 60;
      const _day = _hour * 24;

      let days = Math.floor(this.endsIn / _day);
      let hours = Math.floor((this.endsIn % _day) / _hour);
      let minutes = Math.floor((this.endsIn % _hour) / _minute);
      let seconds = Math.floor((this.endsIn % _minute) / _second);

      hours = hours < 10 ? '0' + hours : hours;
      minutes = minutes < 10 ? '0' + minutes : minutes;
      seconds = seconds < 10 ? '0' + seconds : seconds;
      
      let time = "";
      if (days > 0) {
        time += days + ' day ';
      }
      time += hours + ':';
      time += minutes + ':';
      time += seconds;

      document.querySelector(".product-summary .badge--sale").textContent = `Sale ends in ${time}`;
    }
  }

  customElements.define("campfire-daily-badge", CampfireDailyBadge);
</script>

