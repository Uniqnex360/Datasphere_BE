  const deals = {
    "all": {
      date: "2023-11-23T00:00:00.000-06:00",
      expire: "2023-11-28T09:00:00.000-06:00",
      message: "BLACK FRIDAY BUNDLES"
    }
  };

  let announcementEndsIn = null;

  let announcementCounter = null;

  const startAnnouncementTimer = () => {
    announcementEndsIn -= 1000;

    if (announcementEndsIn < 1000) {
      window.location.reload();
    }

    const _second = 1000;
    const _minute = _second * 60;
    const _hour = _minute * 60;
    const _day = _hour * 24;

    let days = Math.floor(announcementEndsIn / _day);
    let hours = Math.floor((announcementEndsIn % _day) / _hour);
    let minutes = Math.floor((announcementEndsIn % _hour) / _minute);
    let seconds = Math.floor((announcementEndsIn % _minute) / _second);

    hours = hours < 10 ? '0' + hours : hours;
    minutes = minutes < 10 ? '0' + minutes : minutes;
    seconds = seconds < 10 ? '0' + seconds : seconds;
    
    let time = "";
    if (days > 1) {
      time += days + ' days ';
    } else if (days > 0) {
      time += days + ' day ';
    }
    time += hours + ':';
    time += minutes + ':';
    time += seconds;

    document.querySelector(".announcement-counter").textContent = `${time}`;
    document.querySelector(".announcement-counter").classList.remove("hidden");
  };

  const startAnnouncementCounter = () => {
    clearInterval(announcementCounter);
    announcementCounter = setInterval(startAnnouncementTimer, 1000);
  };

