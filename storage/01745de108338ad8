<script>
    window.addEventListener('load', function() {
        var tz = Intl.DateTimeFormat().resolvedOptions().timeZone,
        loc = ['America'],
        len = loc.length,
        iscl = false;
        while(len--) {
            if (tz.indexOf(loc[len])!=-1) {
                iscl = true;
            }
        }
        if(!iscl) {
            document.getElementById("consent-manager").style.display = 'block';
        }
    });
</script>
