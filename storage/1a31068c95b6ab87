    <div class="outer-pdp-thumbnail-slider">
        <ul class="pdp-thumbnail js__pdp-thumbnail-slider js__pdp-thumbnail mt-45">
            
              
              
              <li class="blue">
                  <div class="image-section">
                      <img class="lazyload" data-src="//lifestraw.com/cdn/shop/products/LifeStrawonGreyw-Shadows-3_100x.jpg?v=1750227239" alt="LifeStraw Straw LifeStraw - Online store Blue">
                  </div>
              </li>
            
              
              
              <li class="all">
                  <div class="image-section">
                      <img class="lazyload" data-src="//lifestraw.com/cdn/shop/files/ls-navy_100x.jpg?v=1757602926" alt="LifeStraw Straw LifeStraw - Online store">
                  </div>
              </li>
            
              
              
              <li class="navy">
                  <div class="image-section">
