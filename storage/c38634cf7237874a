<script>
  function erase_input_text(inputId){
    const input = document.getElementById(inputId);
    if (input) {
      input.value = '';
      input.dispatchEvent(new Event('input', { bubbles: true }));
      input.dispatchEvent(new Event('change', { bubbles: true }));
    }
  }
</script>

