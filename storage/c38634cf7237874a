  // Define the target elements (the main sale price and compare price)
  const mainPriceElement = document.getElementById('sale-price');
  const mainComparePriceElement = document.getElementById('compare-price');

  if (mainPriceElement) {
    // Configuration for the MutationObserver
    const config = { childList: true, subtree: true, characterData: true };

    // Function to update the mobile sale price
    function syncSalePrice() {
      const mobilePriceElement = document.getElementById('sale-price-mobile');

      if (mobilePriceElement) {
        mobilePriceElement.innerHTML = mainPriceElement.innerHTML;
      } else {
        console.error('Mobile sale price element not found.');
      }
    }

    // Function to update the mobile compare price
    function syncComparePrice() {
      const mobileComparePriceElement = document.getElementById('compare-price-mobile');

      if (mobileComparePriceElement) {
        mobileComparePriceElement.innerHTML = mainComparePriceElement.innerHTML;
      } else {
        console.error('Mobile compare price element not found.');
      }
    }

    // Create a MutationObserver to watch for changes in the main price element
    const observer = new MutationObserver(function(mutationsList) {
      for (const mutation of mutationsList) {
        if (mutation.type === 'childList' || mutation.type === 'characterData') {
          // Sync the mobile price and compare price whenever the main prices change
          syncSalePrice();
          syncComparePrice();
        }
      }
    });

    // Start observing the main price element
    observer.observe(mainPriceElement, config);

    // Also observe the compare price element for changes
    if (mainComparePriceElement) {
      observer.observe(mainComparePriceElement, config);
    } else {
      console.error('Main compare price element not found.');
    }

    // Initial synchronization in case the content is already present
    syncSalePrice();
    syncComparePrice();
  } else {
    console.error('Main sale price element not found.');
  }

  
  // Listen for native variant change events from the theme
  document.addEventListener('variant:change', function(event) {
    const variant = event.detail?.variant;
  
    if (variant && variant.id) {
      updateSelectedVariant(variant.id); // This updates everything: options, button states, quantity check
    } else {
      console.warn('[variant:change] No valid variant in event detail.');
    }
  });


  function updateSelectedVariant(variantId) {
    const queryString = window.location.search;
    const urlParams = new URLSearchParams(queryString);
    urlParams.set('variant', variantId);
    const newUrl = window.location.pathname + '?' + urlParams.toString();
    window.history.replaceState({}, '', newUrl);

    updateVariantNameAndOptions(variantId);
    updateVariantInputField(variantId); // Update the hidden input field with the selected variant ID

    // Update Add to Cart button based on availability
    updateAddToCartButton(variantId);

    checkQuantityLimit(variantId);
  }

  function updateVariantNameAndOptions(variantId) {
    const variant = getVariantById(variantId);
    
    if (variant) {
      variant.option_positions.forEach(function(position, index) {
        // Skip if position is null
        if (position === null) {
          console.warn(`Skipping position at index ${index} because it is null.`);
          return;
        }
        const option = window.product.options.find(opt => opt.position === position);
        if (option) {
          option.selected_value = decodeHtmlEntities(variant.option_values[index]); // Decode HTML entities
        } else {
          console.error(`Option with position ${position} not found.`);
        }
      });

      // Call to update DOM elements
      updateVariantOptionValues();
      /*updateDiscountButtonPrices(variantId);*/
    } else {
      console.error(`Variant with ID ${variantId} not found.`);
    }
  }

  function updateVariantOptionValues() {
  
    window.product.options.forEach(function(option) {
  
      // Check for variant-option-value elements
      const optionSelector = `variant-option-value[for="option${option.position}"]`;
      const optionElements = document.querySelectorAll(optionSelector);
  
      if (optionElements.length > 0) {
        optionElements.forEach(function(optionElement) {
          if (optionElement) {
            optionElement.textContent = decodeHtmlEntities(option.selected_value);
          } else {
            console.error(`Option element for position ${option.position} not found.`);
          }
        });
      } else {
        // No variant-option-value elements, check for popover
        const sectionElement = document.querySelector(`[data-section-id]`);
        const sectionId = sectionElement ? sectionElement.getAttribute('data-section-id') : null;
  
        // Construct the popover_id
        if (sectionId) {
          const popoverId = `popover-variant-dropdown-${sectionId}-${window.product.id}-${option.position}`;
          const selectedValue = option.selected_value;
  
          // Update the selected value in the popover button
          const selectedValueElement = document.querySelector(`#${popoverId}-selected-value`);
          if (selectedValueElement) {
            selectedValueElement.textContent = decodeHtmlEntities(selectedValue);
          }
  
          // Now handle the option buttons inside the popover
          const optionButtons = document.querySelectorAll(`#${popoverId} .h-stack`);
          optionButtons.forEach(function(button) {
            const buttonValue = button.getAttribute('value');
            if (buttonValue === selectedValue) {
              // Mark this button as selected
              button.setAttribute('aria-selected', 'true');
              button.classList.add('selected');
  
              // Simulate a click on this button to trigger any associated logic (like price update)
              button.click();
            } else {
              // Mark all other buttons as unselected
              button.setAttribute('aria-selected', 'false');
              button.classList.remove('selected'); // Remove any selected class
            }
          });
  
          // Update the hidden input inside the popover to reflect the selected value
          const inputElement = document.querySelector(`#${popoverId}-input`);
          if (inputElement) {
            inputElement.value = selectedValue;
          }
        } else {
          console.error(`Section ID not found for option position ${option.position}.`);
        }
      }
    });
  }

  function updateVariantInputField(variantId) {
    // Retrieve the form ID from the buy-buttons element
    const buyButtons = document.querySelector('.buy-buttons');
    const formId = buyButtons.getAttribute('form');

    if (formId) {
      // Use the form ID to locate the form
      const form = document.querySelector(`#${formId}`);
      if (form) {
        // Locate the variant input field within the form
        const variantInputField = form.querySelector('input[name="id"]');
        if (variantInputField) {
          variantInputField.value = variantId;  // Update the value of the variant ID
        } else {
          console.error('Variant input field not found within form.');
        }
        
        // Locate the hidden input field for the price within the form
        const priceInputField = form.querySelector('input[name="price"]');
        if (priceInputField) {
          // Assuming the price needs to be updated based on the selected variant
          const variant = getVariantById(variantId);  // Fetch the variant object using the variant ID
          if (variant) {
            const updatedPrice = (variant.price / 100).toFixed(2);
            priceInputField.value = updatedPrice;
          } else {
            console.error('Variant not found for updating price.');
          }
          
        } else {
          console.error('Price input field not found within form.');
        }
      } else {
        console.error('Form not found.');
      }
    } else {
      console.error('Form ID not found in buy-buttons.');
    }
  }

  function getVariantById(variantId) {
    const variant = window.product.variants.find(variant => variant.id === Number(variantId));
    return variant;
  }

  function getCurrentSelectedVariantId() {
    const variantInput = document.querySelector('input[name="id"]');
    return variantInput ? parseInt(variantInput.value, 10) : null;
  }  

  function updateAddToCartButton(variantId) {
    const variant = getVariantById(variantId);
    const addToCartButton = document.querySelector('.v-stack .buy-buttons button[type="submit"]');
    const backInStockMessage = document.querySelector('.back-in-stock-message');
    
    if (variant && variant.available) {
      addToCartButton.removeAttribute('disabled');
      addToCartButton.textContent = 'Add to cart';     
      if (backInStockMessage) {
        backInStockMessage.remove();
      }
    } else {
      addToCartButton.setAttribute('disabled', 'disabled');
      addToCartButton.textContent = 'Sold out';

      const backInStockDate = variant.metafields?.back_in_stock || null;
      let messageText;

      if (variant.metafields?.back_in_stock) {
        const today = new Date();
        const stockDate = new Date(variant.metafields.back_in_stock);
  
        // Check if the back-in-stock date is today or in the past
        if (stockDate <= today) {
          messageText = 'Estimated back in stock date: TBA';
        } else {
          messageText = `Estimated back in stock date: ${stockDate.toLocaleDateString()}`;
        }
      } else {
        messageText = 'Estimated back in stock date: TBA';
      }
  
      // Update or create back-in-stock message
      if (backInStockMessage) {
        backInStockMessage.textContent = messageText;
      } else {
        const span = document.createElement('span');
        span.className = 'back-in-stock-message';
        span.textContent = messageText;
        addToCartButton.parentNode.insertBefore(span, addToCartButton);
      }      
    }
    if (typeof checkQuantityLimit === 'function') {
      checkQuantityLimit(variantId);
    }    
  }

  const swatchOptions = document.querySelectorAll('#SingleOptionSelector-1 input');
  swatchOptions.forEach(function(option){
    option.addEventListener('click', function() {
      const selectedVariantId = option.getAttribute('data-variant-id');
      if (selectedVariantId){
        // Get the corresponding dropdown item
        const dropdownItem = document.querySelector(`li[data-variant-id="${selectedVariantId}"]`);
        if (dropdownItem) {
          updateDropdownDisplay(dropdownItem);
        }
      }
    });
  });
  
  // Event listeners for dropdown options
  const dropdownOptions = document.querySelectorAll('#color-options li');
  dropdownOptions.forEach(function(option) {
    option.addEventListener('click', function() {
      const selectedVariantId = option.getAttribute('data-variant-id');
      if (selectedVariantId) {
        // Optionally, close the dropdown after selection
        dropdown();
        selectDropdownColor(selectedVariantId);
        updateSelectedVariant(selectedVariantId);

      } else {
        console.error('No variant ID found for the clicked dropdown option.');
      }
    });
  });  

  
  // Event listeners for thumbnail gallery items & Media changes
  const variantImages = document.querySelectorAll('.product-gallery__thumbnail');
  
  variantImages.forEach((image) => {
    // Shared logic for handling variant updates
    function handleVariantUpdate() {
      const selectedVariantId = image.getAttribute('data-variant-id');
      const selectedVariantPrice = image.getAttribute('data-variant-price');
  
      if (selectedVariantId) {
        selectDropdownColor(selectedVariantId);
        updateSelectedVariant(selectedVariantId);
        quantityButtonNew(selectedVariantPrice);
      } else {
        console.error('No variant ID found for the element.');
      }
    }
  
    // Click event listener
    image.addEventListener('click', handleVariantUpdate);
  
    // MutationObserver for aria-current
    const observer = new MutationObserver((mutationsList) => {
      mutationsList.forEach((mutation) => {
        if (mutation.type === 'attributes' && mutation.attributeName === 'aria-current') {
          const isAriaCurrent = image.getAttribute('aria-current') === 'true';
          if (isAriaCurrent) {
            handleVariantUpdate(); 
          }
        }
      });
    });
  
    // Start observing the image element for attribute changes
    observer.observe(image, { attributes: true });
  });



  function quantityButtonNew(price){
    let total = document.getElementById('totalCostQ');
      // Calculate the updated total cost and discount
    let quantity = Number(document.getElementById('Quantity')?.value) || 1;
    let discount = quantityBreakCalculation(quantity);
    let totalCost = quantity * price/100 * (1 - discount);
    document.getElementById('totalCostQ').textContent = '$' + totalCost.toFixed(2);
    var productMeasurement = "Yard";
    updateDiscountUI(discount, price/100, quantity, productMeasurement);
    price = parseFloat(price.replace(/[^\d.]/g, ''));
    hideDiscountText();
  }  

/* This is in product-info-marine-vinyl.liquid already, not sure why - leftover from previous company. CLEAN UP CODE.
function updateDiscountUI(discount, unitPrice, quantity, productMeasurement) {
  let discountedUnitPrice = unitPrice * (1 - discount);

  if (discount > 0) {
    document.getElementById('discountedUnitPrice').textContent = '$' + discountedUnitPrice.toFixed(2) + ' per ' + productMeasurement;
    document.getElementById('discountedUnitPrice').style.display = 'inline';
    document.getElementById('unitPriceDisplay').innerHTML = '<span style="color: #ff0000">Your price:</span><s>$' + unitPrice.toFixed(2) + '</s>';
    document.getElementById('savingsDisplay').textContent = (discount * 100) + '% volume discount applied';
  } else {
    document.getElementById('discountedUnitPrice').textContent = '';
    document.getElementById('discountedUnitPrice').style.display = 'none';
    document.getElementById('unitPriceDisplay').innerHTML = 'Unit price: $' + unitPrice.toFixed(2) + ' per ' + productMeasurement;
    document.getElementById('savingsDisplay').textContent = '';
  }
}
*/

  function hideDiscountText() {
    let discounttext = document.getElementById('discount-text');
    if (discounttext) {
        discounttext.style.display = 'none';
    }
  }

  
  // Function to handle variant selection from dropdown
    function changeVariant(element) {
      const selectedVariantId = element.getAttribute('data-variant-id');
      if (selectedVariantId) {
        updateSelectedVariant(selectedVariantId);
        selectDropdownColor(selectedVariantId);    
        dropdown(); // Close dropdown after selection
      } else {
        console.error('No variant ID found for the selected dropdown item.');
      }
    }



  function selectDropdownColor(id) {
    let selectOptionColor = colorToVariantMap[id];

    // Find the first button element with the matching value attribute
    const button = document.querySelector('button[value="' + selectOptionColor + '"]');
    
    // Check if the button exists before calling click()
    if (button) {
      button.click();
    }

    // Select the input element with the name 'option2' and the specified value
    var element = document.querySelector('input[name="option2"][value="' + selectOptionColor + '"]');
    
    // Check if the element exists before calling click()
    if (element) {
      element.click();
    }
    // Get the corresponding dropdown item
    const dropdownItem = document.querySelector(`li[data-variant-id="${id}"]`);
    if (dropdownItem) {
      updateDropdownDisplay(dropdownItem);
    }
  }

  
  /* The following code concerns minimum quantities for certain products. The 'add to cart' button is disabled
    if quantity in the quantity box + cart are below the specified threshold. See metafields in product pages for enabling. */ 

  const productId = 2804694083;

  const productSection = document.querySelector(`.product-section[data-custom-product-id="${productId}"]`);

  // Define the target elements (the main sale price and compare price)
  const quantityInput = productSection.querySelector('quantity-selector input[type="number"]');
  const decreaseButton = productSection.querySelector('quantity-selector button[data-action="decrease"]');
  const increaseButton = productSection.querySelector('quantity-selector button[data-action="increase"]');
  
  // Select the Add to Cart button
  const addToCartButton = productSection.querySelector('buy-buttons button[type="submit"]');
  
  const MAX_ALLOWED = 0; 
  const enableMinimumQuantity = false;

  
  async function checkQuantityLimit(variantId) {
  
    const variant = getVariantById(variantId);
    if (!variant || !variant.available) {
      return; // Don't run the minimum quantity logic if variant is out of stock or unavailable
    }
    
    // Only proceed if the minimum quantity feature is enabled and the value is greater than 0
    if (enableMinimumQuantity && MAX_ALLOWED > 0) {
      const selectedQty = parseInt(quantityInput.value, 10) || 0;
      //const variantId = getCurrentSelectedVariantId();
    
      if (!variantId) {
        console.warn('No variant selected yet.');
        return;
      }
    
      const isInCart = await isVariantInCartAlready(variantId);
    
      if (isInCart) {
        addToCartButton.disabled = false;
        addToCartButton.textContent = "Add to cart";
      } else {
        if (selectedQty < MAX_ALLOWED) {
          // Safeguard in case the item is sold out, on top of the safeguards above
          if (addToCartButton.textContent === 'Sold out') {
            return;
          }
          addToCartButton.disabled = true;
          addToCartButton.textContent = `Minimum of ${MAX_ALLOWED} units required`;
        } else {
          addToCartButton.disabled = false;
          addToCartButton.textContent = "Add to cart";
        }
      }
    } else {
      console.log('Minimum quantity feature is not enabled or the minimum quantity is set to 0.');
    }
  }


    // Trigger check on input change (when typing in the quantity box)
    quantityInput.addEventListener('input', async function() {
      const variantId = getCurrentSelectedVariantId();
      await checkQuantityLimit(variantId);
    });

    // Trigger check on increase button click
    increaseButton.addEventListener('click', async function() {
      const variantId = getCurrentSelectedVariantId();
      /* Below, delays reading the quantity value until after the input has been updated, 
       to account for the functions in product-info-marine-vinyl such as quantityChangeNew */    
      setTimeout(checkQuantityLimit(variantId), 0);
      //await checkQuantityLimit(variantId);
    });

    // Trigger check on decrease button click
    decreaseButton.addEventListener('click', async function() {
      const variantId = getCurrentSelectedVariantId();
      /* Below, delays reading the quantity value until after the input has been updated, 
       to account for the functions in product-info-marine-vinyl such as quantityChangeNew */    
      setTimeout(checkQuantityLimit(variantId), 0);
      //await checkQuantityLimit(variantId);
    });

    // Initial check
    const variantId = getCurrentSelectedVariantId();
    checkQuantityLimit(variantId);

    // Listen for cart updates (e.g., after item removal) to handle disabling button again
    document.addEventListener('cart:change', async function(event) {
      const variantId = getCurrentSelectedVariantId();
      await checkQuantityLimit(variantId);
    });

    // Prevent adding items to cart if quantity is less than the minimum
    addToCartButton.addEventListener('click', async function(event) {
      const selectedQty = parseInt(quantityInput.value, 10) || 0;
      const variantId = getCurrentSelectedVariantId();
      const isInCart = await isVariantInCartAlready(variantId);
    
      if (!isInCart && selectedQty < MAX_ALLOWED) {
        event.preventDefault();
        // Optionally show a user-friendly error
      }
    });

  // Fetch cart info
  async function getCart() {
    const response = await fetch('/cart.js');
    const cart = await response.json();
    return cart;
  }

  // Check if the product is already in the cart
  /*
  async function isProductInCartAlready(productId) {
    const cart = await getCart();
    let productInCart = false;

    cart.items.forEach(item => {
      if (item.product_id === productId) {
        productInCart = true; // If product is in cart, set flag to true
      }
    });

    console.log('Is product in cart:', productInCart);
    return productInCart;
  } */
  
  async function isVariantInCartAlready(variantId) {
    const cart = await getCart();
    const numericId = Number(variantId);
    return cart.items.some(item => item.variant_id === numericId);
  }

  /* -----------------------------
  *  Variant Sub-Gallery (Metafields Only)
  * ----------------------------- */
  const subGallery = document.getElementById('custom-sub-gallery');
  const mainCarousel = document.querySelector('[id^="product-gallery-carousel"]');

  // Build thumbnail area from metafields
  function updateSubGallery(images) {
    // Clear previous thumbnails
    subGallery.innerHTML = '';

    if (!images || images.length < 2) {
      // Hide container if not enough images
      subGallery.style.display = 'none';
      return;
    }

    // Show container and build thumbnails
    subGallery.style.display = 'flex';

    images.slice(0, 4).forEach((img) => {
      const thumb = document.createElement('img');
      thumb.src = img.src || img;
      thumb.setAttribute('data-full-src', img.src || img);
      thumb.alt = img.alt || '';
      thumb.loading = 'lazy';
      thumb.className =
        'cursor-pointer object-cover w-full h-auto rounded border hover:opacity-80 transition';
      subGallery.appendChild(thumb);
    });
  }

  // Detect currently active slide in the carousel
  function getActiveSlide() {
    const slides = mainCarousel.querySelectorAll('.product-gallery__media');
    const carouselRect = mainCarousel.getBoundingClientRect();

    let closestSlide = null;
    let minDistance = Infinity;

    slides.forEach((slide) => {
      const slideRect = slide.getBoundingClientRect();
      const slideCenter = slideRect.left + slideRect.width / 2;
      const carouselCenter = carouselRect.left + carouselRect.width / 2;
      const distance = Math.abs(slideCenter - carouselCenter);

      if (distance < minDistance) {
        minDistance = distance;
        closestSlide = slide;
      }
    });

    return closestSlide;
  }

  // Update main image for the active slide when a thumbnail is clicked
  subGallery.addEventListener('click', (e) => {
    const img = e.target.closest('img');
    if (!img) return;

    const src = img.getAttribute('data-full-src') || img.src;
    const activeSlide = getActiveSlide();

    if (activeSlide) {
      const mainImg = activeSlide.querySelector('img');
      mainImg.src = src;
      mainImg.srcset = '';
    }
  });

  // Update sub-gallery when a variant is selected
  function updateGalleryForVariant(variantId) {
    const variant = getVariantById(variantId);
    if (!variant) return;

    const extraImages = variant.metafields?.custom?.extra_images || [];
    updateSubGallery(extraImages);
  }

  // Hook into theme's variant:change event
  document.addEventListener('variant:change', (event) => {
    const variant = event.detail?.variant;
    if (variant?.id) {
      updateGalleryForVariant(variant.id);
    }
  });

  // Initial load for the selected variant
  const initialVariantId = getCurrentSelectedVariantId();
  if (initialVariantId) {
    updateGalleryForVariant(initialVariantId);
  }


});

</script>






