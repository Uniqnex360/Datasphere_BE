  // Extract color values from productOptions
  var colorOptions = [];
  
  var colorOption = productOptions.find(option => /color|colour/i.test(option.name));
  
  if (colorOption) {
    colorOptions = colorOption.values;
  }

  // Create a mapping of color values to variant IDs
  var colorToVariantMap = {};

  // Iterate over variants to populate the map
  productVariants.forEach(variant => {
    var color = variant.option2; // Assuming color is in option2
    if (colorOptions.includes(color)) {
      colorToVariantMap[variant.id] = color;
    }
  });

  // Output the mapping for debugging
  console.log('Color to Variant Map:', colorToVariantMap);

  // Map thumbnail variants
  var thumbnails = document.querySelectorAll('.product-gallery__thumbnail');
  thumbnails.forEach(thumbnail => {
    var variantId = thumbnail.getAttribute('data-variant-id');
    if (variantId) {
      // Check if this variant ID exists in the colorToVariantMap
      if (colorToVariantMap[variantId]) {
        thumbnail.setAttribute('data-color', colorToVariantMap[variantId]);
      } else {
        console.warn(`No color found for variant ID ${variantId}`);
      }
    }
  });

</script><variant-picker class="variant-picker v-stack gap-4" handle="marine-vinyl-fabric" form="product-form-2804694083-template--18200218763348__main" update-url >
      

      
 
      
        
      

