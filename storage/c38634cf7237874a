<!-- Stamped Main Script: Begin 
    <script>
      function myInit() {
        StampedFn.init({ apiKey: '75e3281b-dd18-4b29-8efa-b74180881d2f', sId: '331064' });
      }
    </script>
    <script
      async
      onload="myInit()"
      type="text/javascript"
      src="https://cdn1.stamped.io/files/widget.min.js"
    ></script>
    

    <style>
      #stamped-reviews-widget.carousel {
        float: inherit !important;
      }

      #stamped-main-widget {
        min-height:1880px;
      }
    </style>
-->


<script>
  (function() {
    let stampedLoaded = false;
    
    function loadStamped() {
      if (stampedLoaded) return;
      stampedLoaded = true;
      
      const script = document.createElement('script');
      script.src = 'https://cdn1.stamped.io/files/widget.min.js';
      script.async = true;
      script.onload = function() {
        
          StampedFn.init({ 
            apiKey: '75e3281b-dd18-4b29-8efa-b74180881d2f', 
            sId: '331064' 
          });
        
      };
      document.body.appendChild(script);
    }
    
    // ✅ Usa requestIdleCallback para carregar quando browser estiver livre
    if ('requestIdleCallback' in window) {
      requestIdleCallback(loadStamped, { timeout: 3000 });
    } else {
      // Fallback para browsers antigos
      setTimeout(loadStamped, 2000);
    }
    
    // ✅ OU carrega quando usuário interage
    const events = ['scroll', 'mousemove', 'touchstart', 'click'];
    const onInteraction = () => {
      loadStamped();
      events.forEach(event => {
        document.removeEventListener(event, onInteraction);
      });
    };
    
    events.forEach(event => {
      document.addEventListener(event, onInteraction, { 
        once: true, 
        passive: true 
      });
    });
  })();
</script>

