<script>

var b2b_customer = false;

// Function to handle dropdown visibility
function dropdown() {
  let dropList = document.getElementById("color-options");
  const dropListArrow = document.querySelector("#colorSelection svg");
  if (dropList) {
    if (window.getComputedStyle(dropList).display === "none") {
      dropList.style.display = "block";
      dropListArrow.style.transform = "rotate(180deg)";
    } else {
      dropList.style.display = "none";
      dropListArrow.style.transform = "initial";
    }
  }
}

// Function to update the selected dropdown display
function updateDropdownDisplay(selectedItem) {
  // Get the image and label from the selected item
  const selectedImageSrc = selectedItem.querySelector(".dropdown-img").src;
  const selectedLabel = selectedItem.querySelector(".dropdown-label").textContent;
  const selectedVariantId = selectedItem.getAttribute('data-variant-id');

  // Update the main dropdown display
  const dropdownImage = document.querySelector("#colorSelection .dropdown-img");
  const dropdownLabel = document.querySelector("#colorSelection .color-select-label");

  if (dropdownImage && dropdownLabel) {
    dropdownImage.src = selectedImageSrc;
    dropdownLabel.textContent = selectedLabel;
  }

  // Hide the dropdown list
  let dropList = document.getElementById("color-options");
  if (dropList) {
    dropList.style.display = "none";
  }
}

// Attach click event listeners to each dropdown item
document.querySelectorAll(".color-option").forEach(option => {
  option.addEventListener("click", function () {
    updateDropdownDisplay(this);
    dropdown(); // Close the dropdown after selection
  });
});  
  
// Function to handle quantity changes and calculate total cost and discounts
function quantityChangeNew(event, action) {

  event.preventDefault();
  var productMeasurement = "Yard";
  let quantityInput = document.getElementById('Quantity');
  let quantity = parseInt(quantityInput.value);

  // Store the previous quantity for comparison
  let previousQuantity = quantity;

  // Set the updated quantity and prevent triggering `change` again
  quantityInput.value = quantity;

  // Calculate the updated total cost and discount
  let unitPrice = Number(document.getElementById('unitPriceDisplay').textContent.split('$')[1]) || currentVariantPrice();
  let discount = quantityBreakCalculation(quantity);
  let totalCost;
  
  if (!b2b_customer) {
    totalCost = quantity * unitPrice * (1 - discount);
  } else {
    totalCost = quantity * unitPrice;
  }

  // Update the UI elements accordingly
  document.getElementById('totalCostQ').textContent = '$' + totalCost.toFixed(2);
  updateDiscountUI(discount, unitPrice, quantity, productMeasurement);
}

function updateDiscountUI(discount, unitPrice, quantity, productMeasurement) {
  let discountedUnitPrice = unitPrice * (1 - discount);

  if (discount > 0 && !b2b_customer) {
    document.getElementById('discountedUnitPrice').textContent = '$' + discountedUnitPrice.toFixed(2) + ' per ' + productMeasurement;
    document.getElementById('discountedUnitPrice').style.display = 'inline';
    document.getElementById('unitPriceDisplay').innerHTML = '<span style="color: #ff0000">Your price:</span><s>$' + unitPrice.toFixed(2) + '</s>';
    document.getElementById('savingsDisplay').textContent = (discount * 100) + '% volume discount applied';
  } else {
    document.getElementById('discountedUnitPrice').textContent = '';
    document.getElementById('discountedUnitPrice').style.display = 'none';
    document.getElementById('unitPriceDisplay').innerHTML = 'Unit price: $' + unitPrice.toFixed(2) + ' per ' + productMeasurement;
    document.getElementById('savingsDisplay').textContent = '';
  }
}


  // Function to handle button click quantity updates
  function quantityButton(quantity) {
    let total = document.getElementById('totalCostQ');
    let q = document.getElementById('Quantity');
    q.value = quantity;

    // Calculate total cost using quantity break and current variant price
    let discount = quantityBreakCalculation(quantity);
    let newPrice = (1 - discount) * currentVariantPrice() * quantity;
    newPrice = newPrice.toFixed(2);

    // Update the total cost display
    total.innerText = `$ ${newPrice}`;
    quantityChangeNew(new Event('quantityButtonClick'), 'changeQuantityButton');
  }

