<script>
  document.addEventListener('DOMContentLoaded', function() {
    document.querySelectorAll('details').forEach(function(details) {
      details.addEventListener('click', function(event) {
        if (event.target.tagName.toLowerCase() === 'a') {
          let link = event.target;
          if (link) {
            window.location.href = link.href;
          }
        }
      });
    });
  });
</script>
<script>
  document.querySelectorAll('.third-level li').forEach(item => {
    item.addEventListener('click', () => {
      const link = item.querySelector('a');
      if (link) {
        window.location.href = link.getAttribute('href');
      }
    });
  });
</script></x-header>

  <div class="custom__predictive_search_wrapper">
    <predictive-search id="header-predictive-search" class="predictive-search">
      <div class="predictive-search__content" slot="results"></div>
    </predictive-search>
  </div>

</height-observer>

<script>
  document.documentElement.style.setProperty('--header-height', `${document.getElementById('shopify-section-sections--18200210440276__header').clientHeight.toFixed(2)}px`);
</script>



<script>
  document.addEventListener("DOMContentLoaded", function() {
    var drawerBtns = document.querySelectorAll('.drawer__btn');
    var drawerContainers = document.querySelectorAll('.drawer__container');
    var drawerOverlay = document.querySelector('.drawer__overlay');
    var drawerCloseBtn = document.querySelectorAll('.drawer__close__btn');
    var submenuContainers = document.querySelectorAll('.submenu__container');

    // Click event for drawer buttons
    drawerBtns.forEach(function(btn) {
        btn.closest('.drawer__btn').addEventListener('click', function(e) {
            var project = btn.dataset.drawer;
            document.querySelector(".drawer" + project).classList.add('drawer__active');
            drawerOverlay.style.display = 'block';
            setTimeout(function() {
                drawerCloseBtn.forEach(btn => btn.style.display = 'block');
            }, 200);
        });
    });

    // Click event for close buttons
    drawerCloseBtn.forEach(function(btn) {
        btn.addEventListener('click', function(e) {
            var drawerContainer = btn.closest('.drawer__container');
            var project = drawerContainer.dataset.drawer;

            drawerContainer.classList.remove('drawer__active');
            drawerOverlay.style.display = 'none';
            drawerCloseBtn.forEach(btn => btn.style.display = 'none');
            submenuContainers.forEach(function(submenu) {
                submenu.style.display = 'none';
            });
        });
    });

    // Hover event for second-level menu items (li elements)
    drawerContainers.forEach(function(container) {
        container.querySelectorAll('li').forEach(function(li) {
            li.addEventListener('mouseenter', function(e) {
                var project = li.dataset.drawer;
    
                // Hide all third-level menus except the one associated with the current second-level item
                submenuContainers.forEach(function(submenu) {
                    if (submenu.dataset.drawer !== project) {
                        submenu.style.display = 'none';
                        submenu.classList.remove('drawer_active');
                    }
                });
    
                // Show the third-level menu associated with the current second-level item
                var thirdLevelMenu = document.querySelector(".drawers" + project);
                if (thirdLevelMenu) {
                    thirdLevelMenu.style.display = 'block';
                    thirdLevelMenu.classList.add('drawer_active'); // Add active class to third-level menu
                }
            });
    
            li.addEventListener('mouseleave', function(e) {
                var project = li.dataset.drawer;
                var relatedTarget = e.relatedTarget;
    
                // Check if the mouse is leaving to the third-level menu or its associated submenu container
                if (relatedTarget && relatedTarget.closest('.submenu__container') && relatedTarget.dataset.drawer === project) {
                    return;
                }
    
                // Hide the third-level menu associated with the current second-level item
                var thirdLevelMenu = document.querySelector(".drawers" + project);
                if (thirdLevelMenu) {
                    thirdLevelMenu.style.display = 'none';
                    thirdLevelMenu.classList.remove('drawer_active'); // Remove active class from third-level menu
                }
            });
        });
    });


    // Click event outside of drawer containers
    document.addEventListener('mousedown', function(e) {
        var isOutside = true;
        submenuContainers.forEach(function(submenu) {
            if (submenu.contains(e.target) || e.target === drawerCloseBtn) {
                isOutside = false;
            }
        });
        if (isOutside && !Array.from(drawerContainers).some(container => container.contains(e.target))) {
            drawerContainers.forEach(function(container) {
                container.classList.remove('drawer__active');
            });
            submenuContainers.forEach(function(submenu) {
                submenu.style.display = 'none';
            });
            drawerOverlay.style.display = 'none';
            /* drawerCloseBtn.style.display = 'none'; //M */
            drawerCloseBtn.forEach(btn => btn.style.display = 'none');
        }
    });
});
</script><style> #shopify-section-sections--18200210440276__header .header {background: #f8f8f8; padding: 12px 30px 0 30px;} #shopify-section-sections--18200210440276__header .custom__sub-dropdown-menu-8 {max-height: calc( 100vh - var(--header-height, 0px) - var(--announcement-bar-height, 0px) - 210px ); overflow-y: scroll;} </style></header>
<!-- END sections: header-group --><!-- BEGIN sections: overlay-group -->
