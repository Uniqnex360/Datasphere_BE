                function toggleProp65() {
                  var prop65Div = document.getElementById("prop65");
                  if (prop65Div.style.display === "none") {
                    prop65Div.style.display = "block";
                  } else {
                    prop65Div.style.display = "none";
                  }
                }
              </script>
