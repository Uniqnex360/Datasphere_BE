<script type="7208b0bdba1721bf03699347-text/javascript">
    require(["jquery"],function($){
        $('body').on("click",".minusQty, .addQty",function(){
            var obj = $(this);
            var currentQty = obj.siblings('.cart-item-qty').val();
            var productId = obj.parent().children('input[name="qty"]').attr("data-cart-item");

            if(obj.hasClass('addQty')){
                var newAdd = parseInt(currentQty) + parseInt(1);
                obj.siblings('.cart-item-qty').val(newAdd);
                obj.siblings('.cart-item-qty').attr('data-item-qty',newAdd);
                $('#update-cart-item-'+productId).show();
            } else {
                if(parseInt(currentQty) > 1){
                    var newAdd = parseInt(currentQty) - parseInt(1);
                    obj.siblings('.cart-item-qty').val(newAdd);
                    obj.siblings('.cart-item-qty').attr('data-item-qty',newAdd);
                    $('#update-cart-item-'+productId).show();
                }
            }
        });
    });
</script>



