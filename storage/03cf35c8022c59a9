<script type="7208b0bdba1721bf03699347-text/javascript">
    (function () {
        var badgeScriptLoaded = false;

        // Non USER_DEFINED: explicit render callback
        
        window.renderBadge = function () {
            var ratingBadgeContainer = document.createElement("div");
            document.body.appendChild(ratingBadgeContainer);

            if (!window.gapi || !window.gapi.load) {
                return;
            }

            window.gapi.load('ratingbadge', function () {
                window.gapi.ratingbadge.render(
                    ratingBadgeContainer,
                    {
                        merchant_id: "245832",
                        position: "BOTTOM_RIGHT"
                    }
                );
            });
        };

        
        function loadGoogleBadgeScript() {
            if (badgeScriptLoaded) {
                return;
            }
            badgeScriptLoaded = true;

            var s = document.createElement('script');
            s.async = true;
            s.defer = true;

                        // Non USER_DEFINED: call renderBadge on load
            s.src = "https://apis.google.com/js/platform.js?onload=renderBadge";
            
            var firstScript = document.getElementsByTagName('script')[0];
            firstScript.parentNode.insertBefore(s, firstScript);
        }

        // Lazy load on first user interaction
        ['scroll', 'mousemove', 'touchstart', 'keydown'].forEach(function (evtName) {
            window.addEventListener(evtName, loadGoogleBadgeScript, {
                once: true,
                passive: true
            });
        });
    })();
</script>
<!-- END GCR Badge Code -->

