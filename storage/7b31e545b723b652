  <script>
    $(document).ready(function(){
      var setTimeS = "5";
      var setTime = 5000;

      var cuountdown;

      $("#SiteNav a.external-link").click(function(e){
        e.preventDefault();
        var link = $(this).attr("href");

        var countdonwNum = setTimeS;

        $("#external-link-popup").show();
        $("#external-link-popup .buttons .continuebtn").attr("href", link);

        cuountdown = setInterval(countdownFunc, 1000);
        function countdownFunc() {
          countdonwNum = countdonwNum - 1;
          if(countdonwNum > 0){
            $("#external-link-popup .countdown .countdown-number").html(countdonwNum);
          } else {
            $("#external-link-popup .countdown .countdown-number").html(0);
            $("#external-link-popup .buttons .continuebtn").trigger('click');
            clearInterval(cuountdown);
            window.open(link, '_blank', '');
          }
        }
      });

      $("#external-link-popup .buttons .continuebtn").click(function(){
        countPopupFormat();
      });

      $("#external-link-popup .close a.close-btn, #external-link-popup .buttons .staybtn").click(function(){
        countPopupFormat();

      });

      function countPopupFormat(){
        clearInterval(cuountdown);
        $("#external-link-popup .countdown .countdown-number").html(setTimeS);
        $("#external-link-popup").hide();
      }
      
    });
  </script>




</div>
  	</div>
