        	var vcaf = $(vca).text();
            $('#inv-ca').val(vcaf); 
	        var colorName;
        if($(this).closest('.swatch-element').hasClass('color')){
          colorName = $(this).closest('.swatch-element.color').find('input').val();
        }
              $(this)
              .closest('.swatch')
              .find('.label span')
              .text(optionValue);
              
              if($(this).closest('.swatch').hasClass('swatch-quickshop')) {
                $('.product-template__container form .swatch-element input[value="'+ optionValue +'"]').trigger('click');
              } else {
                selectUpdate(colorName);
