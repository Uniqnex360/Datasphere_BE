            $('#price-proca').val(pcapf); 
			var npcap = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .ncacurpro";
        	var npcapf = $(npcap).text();
              console.log(npcap);
              console.log(npcapf);
            $('#nprice-proca').val(npcapf); 
              
			var vus = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .usinv";
        	var vusf = $(vus).text();
            $('#inv-us').val(vusf); 
              
