<script>
  $(document).ready(function(){
    // magic scroll secondary
    const postDetailsS = document.querySelector(".stickytime");
    const postSidebarS = document.querySelector(".team-list-content-head");
    const postSidebarContentS = document.querySelector(".team-list-content-head > .row");
    const controllers = new ScrollMagic.Controller();
    const scenes = new ScrollMagic.Scene({
      triggerElement: postSidebarS,
      triggerHook: 0,
      duration: getDurationS
    }).setClassToggle("body", "product-header-active") // add class toggle
    .addTo(controllers);
    if (window.matchMedia("(max-width: 992px)").matches) {
      scenes.setPin(postSidebarS, {pushFollowers: false});
    }
    // in your projects, you might want to debounce resize event for better performance
    window.addEventListener("resize", () => {
                            if (window.matchMedia("(max-width: 992px)").matches) {
      scenes.setPin(postSidebarS, {pushFollowers: false});
    } else {
      scenes.removePin(postSidebarS, true);
    }
    });
    function getDurationS() {
      return postDetailsS.offsetHeight - postSidebarContentS.offsetHeight;
    }
    function sizeAll() {
      var	w = window.innerWidth;
      if ( w <= 991) {
        //scene.enabled(false);
        scenes.enabled(true);
      } else {
        //scene.enabled(true);
        scenes.enabled(false);
      }
    }
    $(window).resize(sizeAll);
    sizeAll();
  });
  
  $('.quickshop').magnificPopup({
    type:'inline',
    preloader: false,
    callbacks: {
      open: function(){
      }
    }
  })

	$('.vertical-thumbs').on('click','.product-single__thumbnail',function() {
          if( $(window).width() < 600 ) {
          	var bg = $(this).css('background-image');
        	bg = bg.replace('url(','').replace(')','').replace('1000x','800x').replace(/\"/gi, "");
   			console.log(bg);
         	$.magnificPopup.open({  
    			items: {
      				src: bg
    			},
    			type: 'iframe'
			});
          } else {
         	var bg = $(this).css('background-image');
        	bg = bg.replace('url(','').replace(')','').replace('1000x','2000x').replace(/\"/gi, "");
   			console.log(bg);
          	$.magnificPopup.open({  
              items: {
                  src: '<div class="white-popup"><img src="'+bg+'"></div>',
                  type: 'inline'
              }
			});
         }
	}); 

  

</script>


  <script type="text/javascript">
    function slider_nav_update(){
      $('.product-single__thumbnail:visible').each(function(index){
        if(isScrolledIntoView($(this))) {
          $('.product-slider-nav li').removeClass('active').eq(index).addClass('active');
          return false;
        }
      })
    }
    function isScrolledIntoView(elem){
      var docViewTop = $(window).scrollTop();
      var docViewBottom = docViewTop + $(window).height();

      var elemTop = $(elem).offset().top+100;
      var elemBottom = elemTop + $(elem).height();

      return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
    }
    function slideshowInit(tot) {
      $('.product-thumbs-mobile').slick({
        slidesToShow: tot,
        arrows: false,
        dots: false,
        vertical: true,
        responsive: [
          {
            breakpoint: 992,
            settings: {
              slidesToShow: 1,
              dots: false,
              arrows: true,
              vertical: false
            }
          }
        ]
      });
    }
    function updateNavBullets(count) {
      var addVideo = false,
          $sliderNav = $('.product-slider-nav');
      if($('.nav-list-video').length > 0) {
        addVideo = true
      }
      $sliderNav.html('');
      for(li = 0; li < count; li++) {
        $sliderNav.append('<li></li>');
      }
      if(addVideo==true) {
        $sliderNav.append('<li class="nav-list-video"><em class="fas fa-play-circle"></em></li>');
      }
      slider_nav_update();
    }
    function selectUpdate(color){
	      console.log(color);
      var currentOptions = $.map($('.swatch:not(.swatch-quickshop)[data-option-id="7294588944567"]'), function(element) {
        var $element = $(element),
            currentOption = {};
        currentOption.value = $element.find('input:checked').val();
        currentOption.index = $element.data('index');
        return currentOption;
      });
      var productSingleObject = JSON.parse(document.getElementById('ProductJson-7294588944567').innerHTML);
      var variants = productSingleObject.variants;
      var foundArray = variants.filter(function(variant) {
        return currentOptions.every(function(values) {
          return values.value == variant[values.index];
        });
      });
      if (typeof foundArray !== 'undefined' && foundArray.length > 0){
        // console.log(foundArray);
        $('.product-thumbs-mobile').closest('.thumbs-container').attr('class','thumbs-container');
        // option-color option_name-elmwood option-size option_name-os
        var class_build = '';
        if(foundArray[0].option1 != null) {
          currOptionValue = foundArray[0].option1;
          var handled_class = currOptionValue.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/-$/, '').replace(/^-/, '');
          class_build += ' option-color option_name-' + handled_class;
          if(foundArray[0].option2 != null) {
            currOptionValue = foundArray[0].option2;
            var handled_class = currOptionValue.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/-$/, '').replace(/^-/, '');
            class_build += ' option-size option_name-' + handled_class;
            if(foundArray[0].option3 != null) {
              currOptionValue = foundArray[0].option3;
              var handled_class = currOptionValue.toLowerCase().replace(/[^a-z0-9]+/g, '-').replace(/-$/, '').replace(/^-/, '');
              class_build += ' option- option_name-' + handled_class;
            }
          }
        }
        $('.product-thumbs-mobile').closest('.thumbs-container').addClass(class_build);
        var variantId = foundArray[0].id,
            newurl = window.location.protocol + "//" + window.location.host + window.location.pathname + '?variant='+variantId;
        if(window.location.href != newurl) {
          window.history.replaceState({path:newurl},'',newurl);
          if(!!color || color === '') {
            $('.product-thumbs-mobile').slick('unslick');
            var thumbnailSource = $("#ThumbnailTemplate").html(),
                thumbnailTemplate = Handlebars.compile(thumbnailSource),
                items = [],
                item = {},
                imageJson = JSON.parse(document.getElementById('ImageJSON-7294588944567').innerHTML),
                imageArray = $.grep(imageJson, function (element, index) {
                  return element.option_color == color;
                }),
                images = imageArray[0].images,
                imagesLength = images.length;
            $.each(images, function(index,imageurl) {
              item = {
                image: imageurl
              };
              items.push(item);
            });
            var data = {
              images: items
            }
            $('#thumbs-7294588944567').fadeTo(350, 0, function() {
              $(this).html(thumbnailTemplate(data)).css('opacity',1);
              updateNavBullets(items.length);
              slideshowInit(imagesLength);
            });
          }
        }
      }
    }
    $(document).ready(function() {
      
        var sizehand = $('.swatch-size input:checked').attr('data-variant-handle');
        var colorhand = $('.swatch-color input:checked').attr('data-variant-handle');
      
      
        var dataholderinv = ".op-" +colorhand + sizehand + " .usinv";
      
      var dataintelinv = $(dataholderinv).text();
      if(dataintelinv < 1) {
       // $('.cross-sale-price').hide();
        console.log($('.product-price__sale-label'));
        console.log('cs price:'+ $('.product-single__price .cross-sale-price').text());
        $('.product-price__sale-label').text($('.product-single__price .cross-sale-price').text());
      }
      slideshowInit(1);
      jQuery('.swatch :radio').change(function() {
        console.log("changed");
        var optionIndex = jQuery(this).closest('.swatch').attr('data-option-index');
        var optionValue = jQuery(this).val();
        
        var size = $('.swatch-size input:checked').val();
        var sizehand = $('.swatch-size input:checked').attr('data-variant-handle');
        var color = $('.swatch-color input:checked').val();
        var colorhand = $('.swatch-color input:checked').attr('data-variant-handle');
        
        
        	var dataholderinv = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .usinv";
        	var dataintelinv = $(dataholderinv).text();
        	var dataholdercur = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .uscur";
        	var dataintelcur = $(dataholdercur).html();
        	var dataholdercur2 = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .uscurpro";
        	var dataintelcur2 = $(dataholdercur2).html();              
            var normbase = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .usmsrp";
            var norm_price = $(normbase).html();
            var normbase2 = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .usmsrpflat";
            var norm_price2 = $(normbase2).html();              
              console.log(parseInt(dataintelcur2)+" >= "+parseInt(norm_price2));
                if(parseInt(dataintelcur2) >= parseInt(norm_price2) ){
                  console.log('hid');
                  $('.cross-sale-price').hide();
                } else {
                  console.log('show');
                  $('.cross-sale-price').show();
                  setTimeout(function(){ $('.cross-sale-price').show(); }, 500);
                  $('.cross-sale-price').text(norm_price);
                }

        	$('.template-product .product-single .product-price__price span').html(dataintelcur);        
        	console.log("inv " + dataintelinv + " dataholder" + dataholderinv +" op-" + colorhand + sizehand);
              if (dataintelinv > 0 ){
            	console.log("op3 more than 0");
            	$('.product-form__cart-submit').prop('disabled', false);
        		$('#AddToCartText-7294588944567').text('Add to cart');                                  
            } else {
                  console.log("op3 less than 1")
              $('.product-form__cart-submit').prop('disabled', true);
              
                
                  $('#AddToCartText-7294588944567').text('Out of Stock');                      
                
              
                  
          	  //console.log("less than or equal to 0");
            }
        
            
			var pus = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .uscurreg";
        	var pusf = $(pus).text();
            $('#price-us').val(pusf); 
              
			var pusp = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .uscurpro";
        	var puspf = $(pusp).text();
            $('#price-prous').val(puspf);
              
            // start msrp facilitation
			var camsr = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .camsrp";
        	var camsrf = $(camsr).text();
            $('#price-ca-msrp').val(camsrf);
			var ncamsr = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .ncamsrp";
        	var ncamsrf = $(ncamsr).text();
            $('#nprice-ca-msrp').val(ncamsrf);
                           
			var usmsr = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .usmsrp";
        	var usmsrf = $(usmsr).text();
            $('#price-us-msrp').val(usmsrf);                       
            //end msrp facilitation
              
              
			var pca = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .cacurreg";
        	var pcaf = $(pca).text();
            $('#price-ca').val(pcaf); 
			var npca = ".p-full-finger-sailing-gloves-1 > .op-" +colorhand + sizehand + " .ncacurreg";
