<script type="text/javascript">
    function addToCart(cart_data) {
        var zoovu = document.getElementById("zoovuSku");
        var data_str = encodeURIComponent(JSON.stringify(cart_data));
        zoovu.setAttribute("data-cart", data_str);
        zoovu.click();
    }
</script>
