value]['size']); } } if(_clothingDefinitions[value]['width'].length>0){ if(i == 0){ widthlabel.length = 0; } if(widthlabel.findIndex(element => element.includes(_clothingDefinitions[value]['width']))<0){ widthlabel.push(_clothingDefinitions[value]['width']); } } } i++; }); sizelabel = sizelabel.filter(function (v, i, a) {return a.indexOf(v) == i;}); widthlabel = widthlabel.filter(function (v, i, a) {return a.indexOf(v) == i;}); if(sizelabel.length>1){ _clothingSizeEl.html('Refine by '+sizelabel.join(' / ')); }else{ _clothingSizeEl.html('Refine by '+sizelabel); } if(widthlabel.length>1){ _clothingWidthEl.html('Refine by '+widthlabel.join(' / ')); }else{ _clothingWidthEl.html('Refine by '+widthlabel); } } function _updateFilters(params) { var showShoeFilters = true; var showClothingFilters = true; jQuery.each(_features, function(index, value){ if (typeof(params[value]) != "undefined"){ showClothingFilters = false; } }); if (typeof(params.category) != "undefined"){ showShoeFilters = false; showClothingFilters = false; jQuery.each(params.category, function(index, value){ if(jQuery.inArray(parseInt(value), _clothingCategories)>-1){ showClothingFilters = true; } if(jQuery.inArray(parseInt(value), _shoeCategories)>-1){ showShoeFilters = true; } }); } if (typeof(params.footwear_style) != "undefined"){ showShoeFilters = true; showClothingFilters = false; } if (typeof(params.clothing_style) != "undefined"){ showShoeFilters = false; showClothingFilters = true; } if(showClothingFilters && !showShoeFilters){ jQuery.each(_shoeFilters, function(index, value){ jQuery('#'+value).hide(); jQuery.each(jQuery('#'+value+' ul li span'), function(){ if(jQuery(this).hasClass('checked')){ value = value.replace(/filter-/,''); delete params[value]; jQuery(this).removeClass('checked'); jQuery(this).next().removeClass('selected'); if (jQuery(this).find('input')) { jQuery(this).find('input').prop("checked", false); } } }); }); _updateClothingLabels(params); } else if(showShoeFilters && !showClothingFilters){ jQuery.each(_clothingFilters, function(index, value){ jQuery('#'+value).hide(); jQuery.each(jQuery('#'+value+' ul li span'), function(){ if(jQuery(this).hasClass('checked')){ value = value.replace(/filter-/,''); if(value == 'clothing_style'){value = 'type';} delete params[value]; jQuery(this).removeClass('checked'); jQuery(this).next().removeClass('selected'); if (jQuery(this).find('input')) { jQuery(this).find('input').prop("checked", false); } } }); }); } else { jQuery.each(_shoeFilters, function(index, value){ jQuery('#'+value).show(); }); jQuery.each(_clothingFilters, function(index, value){ jQuery('#'+value).show(); }); } var url = window.location.pathname; url = url.split('/'); if(jQuery.inArray('brands', url)>=0){ jQuery('#filter-brand').hide(); } if(jQuery.inArray('styles', url)>=0){ jQuery('#filter-footwear_style').hide(); } } function _updateProducts() { var params = _searchParams(); if (params == _lastSearch) { return; } _lastSearch = params; _updateSearchLink(); _updateFilters(params); for (var i = 0; i < _requests.length; i++) { _requests[i].abort(); } _requests = []; jQuery('#loading').fadeIn('500'); _requests.push( jQuery.ajax({ type: "POST", url: _opts.searchUrl, data: params, cache: false, success: function (result) { jQuery('#loading').fadeOut('1000'); jQuery(_opts.productContainer).html(result); jQuery('html, body').animate({scrollTop:jQuery(_opts.productContainer).offset().top},500); if (typeof productGridHeader != 'undefined') { jQuery("#page-content h1").html(productGridHeader); Cufon.refresh(); } if (typeof setupCheckboxRadio=='function'){ setupCheckboxRadio(); } if( SearchLoader && typeof SearchLoader.setup==='function'){ SearchLoader.setup(); } } }) ); } function _confirmLogin() { if (jQuery(".employeeConfirmCheckBox").prop("checked")) { jQuery('employee-confirm-button-msg').hide(); jQuery('.storelogin_confirm_confirmation').hide(); jQuery('.storelogin_confirm_info').show(); PS_UI.content.resizePopup(); } else { jQuery('.employe