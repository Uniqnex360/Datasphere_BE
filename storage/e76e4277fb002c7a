state){ var target = el.children('.collapsable'); var label = el.children('label'); var parent = el; if (state=='open') { jQuery(label).addClass('open').attr('aria-expanded','true').removeClass('closed'); jQuery(parent).removeClass('closed'); jQuery(target).removeClass('collapsed'); } else { jQuery(target).addClass('collapsed'); jQuery(label).removeClass('open').attr('aria-expanded','false').addClass('closed'); jQuery(parent).addClass('closed'); } } // OPEN ALL BY DEFAULT jQuery('.product-search .filter-label').each(function(index){  var target = jQuery(this).children('.collapsable'); var parent = jQuery(this); if(target.is(':visible')){ triggerState(jQuery(this),'open'); }else{ triggerState(jQuery(this),'close'); } }); // CLICK EVENTS jQuery('.filter-label > .label').attr({'tabindex':'0','role':'button'}).on('click', function () { var target = jQuery(this).siblings('.collapsable'); var parent = jQuery(this).parent('.filter-label'); if (jQuery(target).hasClass('collapsed')) { triggerState(parent,'open'); } else { triggerState(parent,'close'); } }).on('keypress',function(event){ if(event.which==13){ jQuery(this).trigger('click'); } }); jQuery('#keyword-search').on('click',function () { SHARP.productSearch.search(); }).on('keypress',function(event){ if(event.which==13){ jQuery(this).trigger('click'); } });  if($('.filter-label > .label.open').next().hasClass('collapsed')) { jQuery('.filter-label > .label.open').click(); } // jQuery('.product-search').on('keypress','.cr-input input',function(event){ // if(event.which==13){ // event.preventDefault(); // jQuery(this).trigger('click'); // } // }); // MODIFY WIDTH ? jQuery([ '.product-search #filter-color.filter-label > ul li', '.product-search #filter-size.filter-label > ul li', '.product-search #filter-width.filter-label > ul li' ].join(',')).each(function(index){ var parent = jQuery(this).closest('.filter-label'); triggerState(parent,'open'); va