<script>
if (window.location.pathname.startsWith("/trade-partner-program/")) {
    var _agiIframeLoadedCount = 0;

    function trackFormSubmission() {
        console.log("trackFormSubmission started");

        if (!document.body) {
            console.error("Document body not found. Waiting for DOM to load...");
            return;
        }

        const observer = new MutationObserver(() => {
            const iframe = document.querySelector('iframe[src*="relightdepot/form/"]');

            if (iframe) {
                console.log("Iframe detected:", iframe);
                observer.disconnect();

                // Listen for iframe load event
                iframe.onload = function () {
                    _agiIframeLoadedCount++;

                    //console.log("Iframe load event detected. Load count:", _agiIframeLoadedCount);

                    // Fire GA4 event only on the second load
                    if (_agiIframeLoadedCount === 2) {
                        if (typeof gtag === "function") {
                            gtag("event", "form_submit_trade_partner_program");
                        }
                    }
                };
            }
        });

        observer.observe(document.body, { childList: true, subtree: true });
    }

    document.addEventListener("DOMContentLoaded", trackFormSubmission);
}
</script>
<!-- AGI: END - GA4 Form submission event /trade-partner-program 2025/02/12 -->

