<!-- End Google Tag Manager --><script>
   //Hide Coupon Entry
document.addEventListener("DOMContentLoaded", function () {
    var isQuote = localStorage.getItem("qn_quote_id");
    console.log("Loaded", isQuote);
    if (isQuote) {
        setInterval(function () {
            var toHide = [];
			//Cart Page Coupon Entry
            if (location.pathname.indexOf('/cart.php') === 0) {
                var cartPageCoupon = document.querySelector('li.cart-total--couponCode');
                if (cartPageCoupon) toHide.push(cartPageCoupon);
            }
			//Checkout Coupon Entry
            if (location.pathname.indexOf('/checkout') === 0) {
                var redeemable = document.querySelector('#redeemable-collapsable');
                var redeemableLabel = document.querySelector('.redeemable-label');
                if (redeemable) toHide.push(redeemable);
                if (redeemableLabel) toHide.push(redeemableLabel);
            }
            //Cart Preview Coupon Entry
            if (location.pathname.indexOf('/checkout') !== 0) {
                var cartPviewCoupon = document.querySelector('div._wrapper-coupon-code');
                if (cartPviewCoupon) toHide.push(cartPviewCoupon);
            }
            for (var i = 0; i < toHide.length; i++) {
                if (toHide[i].style) {
                    toHide[i].style.display = 'none';
                }
            }
        }, 250);
    }
});

</script>
