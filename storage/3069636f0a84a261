<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script type="">
document.addEventListener('DOMContentLoaded', function() {
    var tabs = document.querySelectorAll('.tabs .tab');
    var tabContents = document.querySelectorAll('.tab-content');

    // Function to show Resources and Downloads tab and content
    function showResourcesAndDownloadsTab() {
        var resourcesTab = document.querySelector('.tab--resources');
        var resourcesTabContent = document.getElementById('tab-resources-25685');
        var resourcesContentHTML = document.querySelector('.resources_and_downloads').innerHTML.trim();

        // Only show Resources and Downloads tab if content exists
        if (resourcesContentHTML) {
            resourcesTab.style.display = 'block';
            resourcesTabContent.style.display = 'block';
            var productResourcesContent = document.querySelector('.resourses_grid');
            productResourcesContent.innerHTML = resourcesContentHTML;
        } else {
            resourcesTab.style.display = 'none'; // Hide tab if no content
            resourcesTabContent.style.display = 'none';
        }
    }

    // Initialize tab states as needed
    var showResourcesTab = false; // Initially hide Resources and Downloads tab
    showResourcesAndDownloadsTab(); // Call to set initial state

    // Event listener for tab clicks
    tabs.forEach(function(tab) {
        tab.addEventListener('click', function(event) {
            event.preventDefault();
            var tabId = this.querySelector('a').getAttribute('href').replace('#', '');

            // Hide all tab contents
            tabContents.forEach(function(content) {
                content.style.display = 'none';
            });

            // Show selected tab content
            document.getElementById(tabId).style.display = 'block';

            // Handle special case for Resources and Downloads tab
            if (tabId === 'tab-resources-25685') {
                showResourcesAndDownloadsTab();
            }
        });
    });
       if (window.innerWidth <= 768) {
                tabs.forEach(function(tab) {
                tab.addEventListener('click', function(event) {
                    event.preventDefault();
                    var tabId = this.querySelector('a').getAttribute('href').replace('#', '');

                    // Hide all tab contents
                    tabContents.forEach(function(content) {
                        content.style.display = 'block';
                    });

                    // Show selected tab content
                    document.getElementById(tabId).style.display = 'block';

                    // Handle special case for Resources and Downloads tab
                    if (tabId === 'tab-resources-25685') {
                        showResourcesAndDownloadsTab();
                    }
                });
            });
       }

    // Trigger click on first tab to initialize
    tabs[0].click(); // Simulate click on the first tab to initialize tab content visibility
});



</script>
