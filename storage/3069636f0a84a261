        var productId = document.getElementById('tags_field');

        customfield.forEach(keyword => {
            if (keyword.name === '__search_keywords') {

                var keywordsArray = keyword.value.split(',');

                let tags = '';
                keywordsArray.forEach(value => {
                    var trimmedValue = value.trim();
                    var encodedValue = encodeURIComponent(trimmedValue).replace(/%20/g, '+');
                    tags += `<span class="tags_display"><a href="/search.php?search_query=${encodedValue}&section=product"><i class="fa fa-tag"></i>${trimmedValue} </a></span>`;
                });

                productId.innerHTML = tags;
            }
        });
