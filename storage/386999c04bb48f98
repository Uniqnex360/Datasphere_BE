            #otherproductconfigurations.__hide .__toggle {
                display: none
            }
        </style>
        <div class="mb-5" id="otherproductconfigurations" x-data="{showLimit: false, show: false}" :class="{__hide: !show}">
            <div class="iplv15-toggle-header d-flex align-items-center px-md-3">
                <h2>Additional Options Available</h2>
                <span class="ml-auto small text-danger" x-show="showLimit && show" x-on:click="show = false" x-on:keydown.enter="show = false" tabindex="0" role="button" aria-label="hide options" x-cloak>Hide</span>
                <span class="ml-auto small text-danger" x-show="showLimit && !show" x-on:click="show = true" x-on:keydown.enter="show = true" tabindex="0" role="button" aria-label="show all options" x-cloak>Show All (4)</span>
            </div>
