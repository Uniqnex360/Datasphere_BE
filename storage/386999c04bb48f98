<script>

        // Disable scroll when focused on a number input.
        $('.delivery-lookup').on('focus', 'input[type=number]', function (e) {
            $(this).on('wheel', function (e) {
                e.preventDefault();
            });
        });

        // Restore scroll on number inputs.
        $('.delivery-lookup').on('blur', 'input[type=number]', function (e) {
            $(this).off('wheel');
        });

        // Disable up and down keys.
        $('.delivery-lookup').on('keydown', 'input[type=number]', function (e) {
            if (e.which == 38 || e.which == 40)
                e.preventDefault();
        });
        //adds hover box to show up on "i" info icon
        $(function () {
            $('.fdn-tooltip').each(function () { $($(this).popover('show').popover('hide').data('bs.popover').tip).addClass('dark-popover'); });
        })

        const noPickUpLocations = false;
        const noServiceLocations = false;

        if (noPickUpLocations) {
            console.error("WARNING - no pickup locations have been defined");
        }

        if (noServiceLocations) {
            console.error("WARNING - no service locations have been defined");
        }
</script>
