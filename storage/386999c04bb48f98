</form>    </div>

    <script>
    // validate form
    function ValidateForm(e) {
        $("#contactUsFormSubmit").attr('disabled', 'disabled').html("<span class=\"spinner-border spinner-border-sm mr-2\" role=\"status\" aria-hidden=\"true\"></span> Sending...");

        var form = $('#contactUsForm')[0];
        $(form).removeClass('was-validated');
        var valid = form.checkValidity();

        if (false) {
            const email = document.getElementById("EmailAddress");
            const phone = document.getElementById("PhoneNumber");

            if ((email.value.length == 0 || email.validity.typeMismatch) && (phone.value.length == 0 || phone.validity.typeMismatch)) {
                email.setCustomValidity(" ");
                phone.setCustomValidity("Phone Number or Email Address is required");
                valid = false;
            }
        }

        if (!valid) {
            e.preventDefault();
            $(form).addClass('was-validated');
            $("#contactUsFormSubmit").removeAttr('disabled').html("Send");
            return false;
        }
        else {
            return true;
        }
    }
    </script>
</div>
    

