<script>
    document.addEventListener('alpine:init', () => {
        Alpine.data('collapseSection', (collapsibleOption) => ({
            show: true,
            isCollapsible: collapsibleOption !== 0, // 0 = NotCollapsibleShowAlways
            isMobile: (window.matchMedia('(max-width: 767px)').matches),
            init() {
                // Apply collapsible logic for desktop
                if (window.matchMedia('(min-width: 768px)').matches) {
                    if (collapsibleOption === 1) { // CollapsibleOpenByDefault
                        this.show = true;
                    } else if (collapsibleOption === 2) { // CollapsibleCollapsedByDefault
                        this.show = false;
                    }
                } else {
                    this.show = false; // Always collapsible on mobile
                }
            },

            toggleShow() {
                if (this.isCollapsible || this.isMobile) {
                    this.show = !this.show;
                }
            }
        }));
    });
</script>


            
            <div class="js-ajax-partial" data-controller="Rebate" data-action="ListProductRebates" data-route-values="{&quot;mprId&quot;:&quot;4a8d9e07d5ca40fdbbf6aeb2404ce9f9&quot;,&quot;hideFilters&quot;:true}" data-callback="" data-requestpriority="5"></div>

            <div class="mb-5" id="seocopy">
