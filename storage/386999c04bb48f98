<script>
    document.addEventListener('alpine:init', () => {
        Alpine.data('itemPageDisclaimer', () => ({
            _init() {
                const parentDiv = document.getElementsByClassName("item-page-15-disclaimer")[0];
                this.hasDisclaimer = parentDiv.children.length > 0;
            },
            hasDisclaimer: false,
            show: !true
        }));
    });
