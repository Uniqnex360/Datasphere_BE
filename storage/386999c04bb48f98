<div x-data
     x-cloak
     class="hlv6-mask animated fast position-fixed"
     x-show="$store.headerMask.on"
     x-on:click="$store.headerMask.on = false;$store.mobileMenu.toggle(false);$dispatch('mask-clicked')"
     x-transition:enter="fadeIn"
     x-transition:leave="fadeOut"
     role="button"></div>












<script>
    document.addEventListener('alpine:init', () => {
        Alpine.store('mobileMenu', {
            show: false,
            toggle(value) {
                this.show = typeof value !== 'undefined' ? value : !this.show;
                if (this.show) {
                    document.body.classList.add('__menu-open');
                } else {
                    document.body.classList.remove('__menu-open');
                    document.dispatchEvent(new Event('mobile-menu-close'));
                }
            }
        })

        Alpine.store('headerMask', {
            on: false,
            hoverIntentTimeout: null,
            hoverIntentTimeout1: null,
            lastTarget: null,
            toggle() {
                this.on = !this.on;
            }
        })

        Alpine.data('headerApp', () => ({
            showSearch: false,
            searchFocus: false,
            isMobile: false,
            prevScrollPos: window.pageYOffset,
            breakpoint: 992,
            init() {
                window.addEventListener('load', () => {
                    this.calculateMobileOffset();
                });

                this.setCurrentBreakpoint();

                window.addEventListener('scroll', () => { this.handleScroll(); });
                window.addEventListener('resize', () => {
                    this.setCurrentBreakpoint();
                    this.handleScroll();
                    this.calculateMobileOffset();
                });

                document.addEventListener('cart-status-mouseenter', () => {
                    Alpine.store('headerMask').on = true;
                });

                document.addEventListener('cart-status-mouseleave', () => {
                    Alpine.store('headerMask').on = false;
                });
            },
            hoverIntent(show, callback, target) {

                if (typeof callback !== 'function') {
                    console.error('Callback must be a function');
                    return;
                }

                clearTimeout(Alpine.store('headerMask').hoverIntentTimeout);

                if (show) {
                    Alpine.store('headerMask').hoverIntentTimeout = setTimeout(() => {
                        if (target) {
                            if (target !== Alpine.store('headerMask').lastTarget) {
                                target.dispatchEvent(new CustomEvent('non-nav-hover', { detail: {}, bubbles: true }));
                                Alpine.store('headerMask').lastTarget = target;
                            }
                        }

                        Alpine.store('headerMask').on = true;
                        callback();
                    }, 100);
                } else {
                    Alpine.store('headerMask').hoverIntentTimeout = setTimeout(() => {
                        Alpine.store('headerMask').on = false;
                        callback();
                    }, 500);
                }

            },
            setCurrentBreakpoint() {
                const isMobile = window.matchMedia('(max-width: 991px)').matches;
                this.isMobile = isMobile;
            },
            handleShowSearch() {
                this.showSearch = !this.showSearch;

                if (this.showSearch) {
                    $('.hlv6-search__text-box').focus();
                } else {
                    $('.hlv6-search__text-box').blur();
                }
            },
            handleScroll() {
                const currentScrollPos = window.pageYOffset;
                const deltaThreshold = 50; // Minimum number of pixels to scroll before triggering the behavior

                if (Math.abs(currentScrollPos - this.prevScrollPos) > deltaThreshold && !this.showSearch) {
                    if (window.innerWidth <= this.breakpoint) {
                        let topOffset = 0;
                        if (this.prevScrollPos > currentScrollPos) {
                            // Scrolling up
                            topOffset = "0";
                        } else {
                            // Scrolling down
                            const headerHeight = document.getElementById("header").offsetHeight;
                            topOffset = `-${headerHeight}`;
                        }
                        document.getElementById("header").style.top = `${topOffset}px`;
                        document.dispatchEvent(new CustomEvent('header-scroll', { detail: { headerOffset: topOffset }, bubbles: true }));
                    } else {
                        // Reset header position on screens larger than the breakpoint
                        document.getElementById("header").style.top = "0";
                    }

                    this.prevScrollPos = currentScrollPos;
                }

                setTimeout(() => {
                    this.calculateMobileOffset();
                }, 200);
            },
            handleMask() {
                if (this.isMobile) {
                    if (!this.showSearch && !Alpine.store('mobileMenu').show) {
                        Alpine.store('headerMask').on = false;
                    } else {
                        Alpine.store('headerMask').on = true;
                    }
                } else {
                    Alpine.store('headerMask').toggle();
                }
            },
            calculateMobileOffset() {

                function calculateHeight() {
                    var mobileMenu = document.querySelector('.hlv6-mobile-menu');
                    var hlv6 = document.querySelector('.hlv6');
                    var rect = hlv6.getBoundingClientRect();

                    if (rect.top == 0)
                        mobileMenu.style.top = (hlv6.offsetHeight) + "px";
                    else
                        mobileMenu.style.top = (rect.top + hlv6.offsetHeight) + "px";
                }

                // Wait for the next animation frame
                window.requestAnimationFrame(calculateHeight);
            },
            focusSearch() {
                setTimeout(() => {
                    this.handleShowSearch();
                }, 250);
            }
        }));

        Alpine.data('headerNav', () => ({
            primary: {"NavLevel0":[{"NavLevel1":[{"NavLevel2":[],"IsTrigger":false,"Label":"Living Room Sets","Url":"https://www.jacksonvillefurnituremart.com/browse/living-room-groups/491","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Sofas & Loveseats","Url":"https://www.jacksonvillefurnituremart.com/browse/sofas-and-loveseats/635","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Sectional Sofas","Url":"/browse/living-room/sectionals/7","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Accent Chairs","Url":"/browse/living-room/all-chairs/4","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Recliners","Url":"/browse/living-room/recliners/36","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Reclining Sofas","Url":"/browse/living-room/reclining-sofas/35","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Sofa Sleepers","Url":"https://www.jacksonvillefurnituremart.com/browse/living-room/sofa-beds/8","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Ottomans & Poufs","Url":"https://www.jacksonvillefurnituremart.com/browse/ottomans-and-poufs/637","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Benches","Url":"/browse/living-room/benches/25","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Coffee Tables","Url":"/browse/accent-tables/cocktail-tables/76","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Side & End Tables","Url":"/browse/accent-tables/end-tables/75","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false},{"NavLevel2":[],"IsTrigger":false,"Label":"Consoles & Accent Cabinets","Url"