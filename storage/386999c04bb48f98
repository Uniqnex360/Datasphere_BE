<script>
    document.addEventListener('alpine:init', () => {
        Alpine.data('addToListApp', () => ({
            loading: false,
            addClick() {
                const self = this;
                self.loading = true;

                // show modal
                $('#ItemPageAddToListModal').modal('show');

                $.ajax({
                    "type": "GET",
                    "url": Utils.BaseURL + "Quicklist/AddToListWidgetV3?gpid=" + 68639206,
                    cache: false,
                    success: function (data) {
                        self.loading = false;

                        $('#ItemPageAddToListContent').html(data);
                    },
                    error: function () {
                        self.loading = false;
                    }
                });
            }
        }));
    });

    if (navigator.share && Modernizr.mq('only screen and (max-width: 47.9375em)')) {
        var shareLink = document.getElementById('share-link');
        var socialMediaLinks = document.getElementById('social-media-links');
        shareLink.classList.remove('d-none');
        socialMediaLinks.classList.remove('d-flex');
        socialMediaLinks.classList.add('d-none');

        shareLink.addEventListener('click', function (event) {
            event.preventDefault(); // Prevent the default link behavior

            var shareData = {
                title: 'Share this Item',
                url: window.location.href
            };

            navigator.share(shareData)
                .then(function () {
                    fdEvents.publish("share", {
                        method: "share_button",
                        content_type: "email",
                        item_id: 68639206,
                    });
                })
                .catch(function (error) {
                    console.error('Error sharing link:', error);
                });
        });
    }


</script>

