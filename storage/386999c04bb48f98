            getNavLinkClassName(navLabel) {
                let formattedLabel = "";

                formattedLabel = navLabel.replace(/ /g, "-").replace(/'/g, "").toLowerCase();

                return "nav-builder-item__" + formattedLabel;
            },
            setCurrentBreakpoint() {
                const isMobile = window.matchMedia('(max-width: 767px)').matches;
                this.isMobile = isMobile;
            },
            setActiveNav(index) {
                this.activeNav = this.activeNav == index ? -1 : index;
            },
            getStorePhoneLink(phone) {
                const cleanPhone = this.getCleanedPhone(phone);
                return "tel:" + cleanPhone;
            },
            getCleanedPhone(phone) {
                return phone.replace(/\D/g, '');
            },
            formattedPhoneNumber(phone) {
                const cleanPhone = this.getCleanedPhone(phone);

                const match = cleanPhone.match(/^(\d{3})(\d{3})(\d{4})$/);

                // If there is a match, format the phone number
                if (match) {
                    return `(${match[1]}) ${match[2]}-${match[3]}`;
                }

                return phone;
            }
        }));
