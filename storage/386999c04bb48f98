        function swiperComponent() {
            return {
                init() {
                    this.initSwiper();

                    // Re-initialize Swiper on window resize
                    window.addEventListener('resize', () => {
                        this.initSwiper();
                    });
                },
                swiper: null,
                showSwiper: false,
                useSwiperOnMobile: true,
                breakpoint: 768,
                initSwiper() {
                    // If carousel display setting is enabled, and screen size is within mobile range,
                    // initialize swiper; otherwise, destroy swiper
                    if (this.useSwiperOnMobile && window.innerWidth <= this.breakpoint) {
                        if (!this.showSwiper) {
                            this.showSwiper = true;

                            this.$nextTick(() => {
                                this.swiper = new Swiper(this.$refs.swiperContainer, {
                                    calculateHeight: true,
                                    preloadImages: false,
                                    slidesPerView: 2,
                                    spaceBetween: 16,
                                    watchOverflow: true,
                                    centerInsufficientSlides: false,
                                    pagination: {
                                        el: ".swiper-pagination"
                                    },
                                    slidesPerGroup: 2
                                });
                            });
                        }
                    } else {
                        this.showSwiper = false;

                        if (this.swiper) {
                            this.swiper.destroy();
                        }
                    }
                }
            };
        }
    </script>

    



