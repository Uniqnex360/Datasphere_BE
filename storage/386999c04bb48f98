            secondary: {"NavLevel0":[{"NavLevel1":[],"IsTrigger":false,"Label":"Clearance","Url":"https://www.jacksonvillefurnituremart.com/clearance/","Icon":"","OpenInNewTab":false,"ShowLink":true,"MegaMenu":false}]},
            init() {
                this.isSingleAndMegaMenu = ((this.primary.NavLevel0.length == 1 && this.primary.NavLevel0[0].MegaMenu) || false) ? true : false;

                document.addEventListener('mobile-menu-close', () => {
                    setTimeout(() => { this.resetNav(); }, 500);
                });
            },
            layout: 1,
            initialHeight: 0,
            activeNav0: null,
            activeNav0Show: null,
            activeNav1: null,
            activeNav1Show: null,
            activeSecondaryNav0Show: null,
            isSingleAndMegaMenu: false,
            mouseEnter(level, index, hasChildren) {
                if (level == 0) {
                    this.activeNav0 = index;
                    this.activeNav1 = null;
                    this.activeNav1Show = null;
                    clearTimeout(Alpine.store('headerMask').hoverIntentTimeout);

                    Alpine.store('headerMask').hoverIntentTimeout = setTimeout(() => {
                        if (this.activeNav0 == index) {
                            document.dispatchEvent(new CustomEvent('nav-hover', { detail: {}, bubbles: true }));
                            this.activeNav0Show = index;
                            Alpine.store('headerMask').on = hasChildren ? true : false;
                        }
                    }, 100);
                }

                if (level == 1) {
                    this.activeNav1 = index;

                    Alpine.store('headerMask').hoverIntentTimeout1 = setTimeout(() => {
                        if (this.activeNav1 == index) {
                            this.activeNav1Show = index;
                        }
                    }, 100);
                }
            },
            mouseLeave(level, index) {
                if (level == 0) {
                    clearTimeout(Alpine.store('headerMask').hoverIntentTimeout);

                    Alpine.store('headerMask').hoverIntentTimeout = setTimeout(() => {
                        if (this.activeNav0 == index) {
                            this.activeNav0 = this.activeNav0Show = null;
                            this.activeNav1 = this.activeNav1Show = null;
                            Alpine.store('headerMask').on = false;
                        }
                    }, 500);
                }
            },
            setActiveNav0(index, event, hasChildren) {
                if (hasChildren) event.preventDefault();

                this.activeSecondaryNav0Show = null;
                this.activeNav0Show = this.activeNav0Show == index ? null : index;

                this.$nextTick(() => {
                    this.scrollIntoView(event.target);
                });
            },
            setActiveSecondaryNav0(index, event, hasChildren) {
                if (hasChildren) event.preventDefault();

                this.activeNav0Show = null;
                this.activeSecondaryNav0Show = this.activeSecondaryNav0Show == index ? null : index;
            },
            setActiveNav1(index, event, hasChildren) {
                if (hasChildren) event.preventDefault();

                this.activeNav1Show = this.activeNav1Show == index ? null : index;

                this.$nextTick(() => {
                    this.scrollIntoView(event.target);
                });
            },
            scrollIntoView(element) {
                if (element) {
                    element.scrollIntoView({ behavior: 'smooth', block: 'start' });
                }
            },
            resetNav() {
                this.activeNav0 = null;
                this.activeNav0Show = null;
                this.activeNav1 = null;
                this.activeNav1Show = null;
                this.activeSecondaryNav0Show = null;
            },
            getNavLinkClassName(navLabel, moreClasses) {
                let formattedLabel = "";

                formattedLabel = navLabel.replace(/ /g, "-").replace(/'/g, "").toLowerCase();
                formattedLabel = "nav-builder-item__" + formattedLabel;

                if (moreClasses)
                    formattedLabel += " " + moreClasses;

                return formattedLabel;
            }
        }));
    });
</script>
