<script>
  document.querySelectorAll('.cql-slide__cover[data-block-id="AYm0wcWNCVjZlRmp1Z__u_image_cCeWNL"] .pause-play-video').forEach(button => {
    button.addEventListener('click', () => {
      const blockId = button.getAttribute('data-target');
      const coverContainer = document.querySelector(`.cql-slide__cover[data-block-id="${blockId}"]`);
      const video = coverContainer?.querySelector(`.video-html[data-block-id="${blockId}"] video`);

      button.classList.toggle('paused');

      if (video.paused == true) {
        video.play();
      } else {
        video.pause();
      }
    });
  });
</script>












