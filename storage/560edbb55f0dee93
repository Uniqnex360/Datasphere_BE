<script>
  document.querySelectorAll('.tabs__nav .tabs__item').forEach((tab) => {
    tab.addEventListener('click', () => {
      document.querySelectorAll('.tabs__nav .tabs__item').forEach((t) => t.classList.remove('tabs__item--active'));
      document.querySelectorAll('.tabs__panel').forEach((p) => p.classList.remove('tabs__panel--active'));
      tab.classList.add('tabs__item--active');
      document.getElementById(tab.dataset.tab).classList.add('tabs__panel--active');
    });
  });

  class productDownloads extends HTMLElement {
    connectedCallback() {
      if (this.children.length === 0) {
        requestAnimationFrame(() => this.renderDownloadJson());
      } else {
        this.renderDownloadJson();
      }
    }

    renderDownloadJson() {
      const json = this.querySelector('script[type="application/json"]');
      if (json) {
        const downloads = JSON.parse(json.textContent);

        let downloadListEl = document.createElement('ul');
        for (const download of downloads) {
          let listItem = document.createElement('li');
          listItem.innerHTML = `<a href="${download.url}" title="${download.alt}" target="_blank">${download.name}</a>`;
          downloadListEl.appendChild(listItem);
        }
        this.innerHTML = '';
        this.appendChild(downloadListEl);
      }
    }
  }

  if (!customElements.get('product-downloads')) {
    customElements.define('product-downloads', productDownloads);
  }

  // create spec table off of JSON from product.metafields.copy.spec_table
  (() => {
    const spec_table_json = document.getElementById('spec-table-json').textContent.trim();
    if (spec_table_json) {
      const specTableJson = JSON.parse(spec_table_json);
      const specTable = document.querySelector('.spec-table');

      const columnCount = Object.values(specTableJson.groups).reduce(
        (total, group) => total + Math.max(group.length, 1),
        0
      );
      const rowCount = specTableJson.rows.length + 2;
      // set grid dimensions based on size of JSON fields
      specTable.style.cssText = `
        --column-count: ${columnCount};
        --row-count: ${rowCount};
      `;

      // Generate Headers for Spec Table
      let subheaders = [];
      for (const [group, columns] of Object.entries(specTableJson.groups)) {
        const headEl = document.createElement('div');
        headEl.classList.add('table-head');
        // have the 'group' head span so that covers all its sub-headers
        headEl.style.cssText = `
          --column-span: ${Math.max(columns.length, 1)};
        `;
        headEl.innerHTML = group;

        if (columns.length === 0) headEl.classList.add('tall'); // take up both header rows if there are no sub headers
        specTable.appendChild(headEl);

        // Gather up the sub headers under the 'group' headers
        for (const column of columns) {
          const subHeadEl = document.createElement('div');
          subHeadEl.classList.add('table-head', 'sub-head');
          subHeadEl.innerHTML = column;

          subheaders.push(subHeadEl);
        }
      }
      specTable.append(...subheaders);

      // Generate Rows for Spec Table
      specTableJson.rows.forEach((row, index) => {
        // flatten object into values array
        const rowValues = Object.values(row).reduce(
          (rowValues, rowGroup) => rowValues.concat(Object.values(rowGroup)),
          []
        );
        const styleDark = index % 2 === 0;

        // Generate each cell for the row
        rowValues.forEach((value) => {
          const cellEl = document.createElement('div');
          cellEl.classList.add('spec-cell');
          cellEl.innerHTML = value;
          if (styleDark) cellEl.classList.add('dark-row');

          specTable.append(cellEl);
        });
      });
    }
  })();
</script>


</div>
</div>
