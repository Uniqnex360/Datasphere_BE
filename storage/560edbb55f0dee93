        z-index: 1000;
        inset: 0;
        justify-content: center;
        align-items: center;
        background: rgba(0, 0, 0, 0.8);
    }
      
    .video-modal.active {
        display: flex;
    }
      
    .video-modal__overlay {
        position: absolute;
        inset: 0;
        background: rgba(0, 0, 0, 0.5);
    }
      
    .video-modal__content {
        position: relative;
        background: var(--color-brand-black);
        border-radius: 8px;
        overflow: hidden;
        max-width: 96rem;
        width: 90%;
        aspect-ratio: 16 / 9;
        z-index: 1001;
        display: flex;
        align-items: center;
        justify-content: center;
    }
      
    .video-modal__video {
        width: 100%;
        height: 100%;
        object-fit: contain;
    }
      
    .video-modal__close {
        position: absolute;
        top: 2rem;
        right: 2rem;
        font-size: 2.8rem;
        color: var(--color-brand-white);
        background: none;
        border: none;
        z-index: 1002;
        cursor: pointer;
    }
</style>

<script>
  document.addEventListener('DOMContentLoaded', () => {
    const modal = document.getElementById('video-modal');
    const modalContent = document.getElementById('video-modal-player');
    const closeBtn = modal.querySelector('.video-modal__close');
    const overlay = modal.querySelector('.video-modal__overlay');

    document.querySelectorAll('.open-video-modal').forEach(button => {
      button.addEventListener('click', () => {
        const blockId = button.getAttribute('data-target');
        const videoHtmlEl = document.querySelector(`.video-html[data-block-id="${blockId}"]`);
        if (videoHtmlEl) {
          modalContent.innerHTML = videoHtmlEl.innerHTML;
          modal.classList.add('active');
        }
      });
    });

    function closeModal() {
      modal.classList.remove('active');
      modalContent.innerHTML = '';
    }

    closeBtn.addEventListener('click', closeModal);
    overlay.addEventListener('click', closeModal);
  });
</script>



    
