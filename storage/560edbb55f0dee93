</style>


</div><div id="shopify-section-template--19606411673851__image_content_slider_znAkwJ" class="shopify-section"><script defer src="https://cdn.jsdelivr.net/npm/swiper@11.2.0/swiper-bundle.min.js"></script>
<link href="https://cdn.jsdelivr.net/npm/swiper@11.2.0/swiper-bundle.min.css" rel="stylesheet">

<link href="//firepitsdirect.com/cdn/shop/t/5/assets/section-image-content-slider.css?v=71789134066145159031754694132" rel="stylesheet" type="text/css" media="all" />






<script type="module">
  import { ThemeEvents, VariantUpdateEvent } from '@theme/events';
  const sectionId = "template--19606411673851__image_content_slider_znAkwJ";
  console.log('events?', ThemeEvents, VariantUpdateEvent);

  function initalizeSlider() {
    const swiper = new Swiper(".content-swiper-template--19606411673851__image_content_slider_znAkwJ", {
      loop: false,
      navigation: {
        nextEl: ".swiper-button-next-template--19606411673851__image_content_slider_znAkwJ",
        prevEl: ".swiper-button-prev-template--19606411673851__image_content_slider_znAkwJ",
      },
      on: {
        init: function () {
          renderDots(this);
        },
        slideChange: function () {
          updateDots(this);
        }
      }
    });

    function seededRandom(seed) {
      const x = Math.sin(seed) * 10000;
      return x - Math.floor(x);
    }

    function renderDots(swiperInstance) {
      const dotContainer = document.getElementById("SliderDots-template--19606411673851__image_content_slider_znAkwJ");
      dotContainer.innerHTML = "";

      const hardcodedPositions = {
        0: { left: 308, top: 31 },
        1: { left: 253, top: 97 },
        2: { left: 271, top: 190 },
      };

      swiperInstance.slides.forEach((_, index) => {
        if (!swiperInstance.slides[index].classList.contains("swiper-slide-duplicate")) {
          const dot = document.createElement("div");
          dot.classList.add("dot");

          const inner = document.createElement("div");
          inner.classList.add("dot-inner");
          dot.appendChild(inner);

          const left = swiperInstance.slides[index].dataset.dotLeft;
          const top = swiperInstance.slides[index].dataset.dotTop;

          if (left && top) {
            dot.style.left = `${left}`;
            dot.style.top = `${top}`;
          } else {
            const top = 10 + seededRandom(index + 1) * 80;
            const left = 10 + seededRandom((index + 1) * 7) * 80;
            dot.style.top = `${top}%`;
            dot.style.left = `${left}%`;
          }

          if (index === swiperInstance.realIndex) {
            dot.classList.add("active");
          }

          dot.addEventListener("click", () => swiperInstance.slideTo(index));
          dotContainer.appendChild(dot);
        }
      });
    }

    function updateDots(swiperInstance) {
      const dotContainer = document.getElementById("SliderDots-template--19606411673851__image_content_slider_znAkwJ");
      const dots = dotContainer.querySelectorAll(".dot");
      dots.forEach((dot, index) => {
        dot.classList.toggle("active", index === swiperInstance.realIndex);
      });
    }
  }

  document.addEventListener("DOMContentLoaded", initalizeSlider);
  document.addEventListener(ThemeEvents.variantUpdate, (event) => {
    const currentSection = document.querySelector(`[data-section-id="${sectionId}"]`);
    const newSection = event.detail.data.html.querySelector(`[data-section-id="${sectionId}"]`);

    if (currentSection && newSection) {
      currentSection.innerHTML = newSection.innerHTML;
      initalizeSlider();
    }
  });

</script>


