<script>
  document.addEventListener('DOMContentLoaded', () => {
    // Modal
    const modal = document.getElementById('customer-service-modal');
    const closeBtn = modal.querySelector('.customer-service-modal__close');
    const overlay = modal.querySelector('.customer-service-modal__overlay');

    document.querySelectorAll('.open-customer-service-modal').forEach(button => {
      button.addEventListener('click', () => {
        modal.classList.add('active');
      });
    });

    function closeModal() {
      modal.classList.remove('active');
      modalContent.innerHTML = '';
    }

    closeBtn.addEventListener('click', closeModal);
    overlay.addEventListener('click', closeModal);

    // Chat
    document.getElementById('start-chat').addEventListener('click', function(e) {
        e.preventDefault();
        GorgiasChat.open();
        closeModal();
    });
  });
</script>



