<script>
  window.addEventListener('message', (event) => {
  if (event.origin !== 'https://formbuilder.hulkapps.com') return;

  if (event.data.type === 'requestHref') {
    event.source.postMessage({
      type: 'parentHref',
      href: window.location.href
    }, event.origin);
  }
});
</script><script> (function () { var s = document.createElement('script'); var h = document.querySelector('head') || document.body; s.src = 'https://acsbapp.com/apps/app/dist/js/app.js'; s.async = true; s.onload = function () { acsbJS.init(); }; h.appendChild(s); })();
window.accessWidgetOptions = {
language: "en",
onReady: function () {
console.log("accessWidget API is ready");
}
};
</script>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    let links = document.querySelectorAll('a[href^="http"]:not([href*="' + window.location.hostname + '"]):not([download])');
    links.forEach(function(link) {
      link.setAttribute('target', '_blank');
      link.setAttribute('rel', 'noopener noreferrer');
    });
  });
</script>


<script>
  const targetDomains = [
    'dc0313-fa.myshopify.com',
    'grohe-production.myshopify.com',
    'grohe.us',
    'grohe.ca'
  ];
    
  function getLocalePrefix() {
    const currentPath = window.location.pathname;
    const localeMatch = currentPath.match(/^\/([a-z]{2}(-[a-z]{2})?)\//);
    return localeMatch ? '/' + localeMatch[1] : '';
  }
    
  const localePrefix = getLocalePrefix();    
  const anchors = document.querySelectorAll('a[href]:not(header a):not(footer a)');
    
  anchors.forEach(function(anchor) {
    const href = anchor.getAttribute('href');
    
    if (href && href.startsWith('http')) {
      let shouldConvert = false;
      let newHref = href;
      
      try {
        const url = new URL(href);
        const hostname = url.hostname;
        
        targetDomains.forEach(function(domain) {
          if (hostname === domain || hostname === 'www.' + domain) {
            shouldConvert = true;
            
            newHref = url.pathname + url.search + url.hash;
            
            if (localePrefix && !newHref.startsWith(localePrefix)) {
              if (newHref.startsWith('/')) {
                newHref = newHref.substring(1);
              }
              newHref = localePrefix + '/' + newHref;
            }
          }
        });
      } catch (e) {
        console.warn('URL handler error:', href, e);
        return;
      }
      
      if (shouldConvert && newHref !== href) {
        anchor.setAttribute('href', newHref);
      }
    }
  });
</script>




