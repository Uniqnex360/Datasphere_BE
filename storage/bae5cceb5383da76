<script>
  document.addEventListener('DOMContentLoaded', function () {
    // Accordion Level 2 (já existe)
    document.querySelectorAll("[id^='accordion-toggle-']").forEach((button) => {
      button.addEventListener('click', () => {
        const id = button.id.replace('accordion-toggle-', '');
        const content = document.getElementById('accordion-content-' + id);
        if (content) {
          content.classList.toggle('hidden');
        }
      });
    });

    // Accordion Level 3 (NOVO)
    document.querySelectorAll("[id^='accordion-toggle-level3-']").forEach((button) => {
      button.addEventListener('click', (event) => {
        event.stopPropagation(); // Impede que o clique afete o link
        const id = button.id.replace('accordion-toggle-level3-', '');
        const content = document.getElementById('accordion-content-level3-' + id);
        if (content) {
          content.classList.toggle('hidden');
        }
      });
    });

    // Ícones de accordion (já existe)
    document.querySelectorAll('[data-accordion-trigger]').forEach((button) => {
      const icon = button.querySelector('svg.accordion-icon');
      if (!icon) return;
      button.addEventListener('click', () => {
        icon.classList.toggle('rotate-180');
      });
    });
  });
</script>
