<script>
  function injectJS(src, inline, callback) {
    var c = document.createElement("script");
    inline ? (c.innerHTML = src) : (c.src = src);
    callback && (c.onload = callback),
    document.body.appendChild(c);
  }

  function injectJSAsync(src, inline = false) {
    return new Promise((resolve, reject) => {
      var script = document.createElement("script");
      
      script.onload = () => {
        console.log('Script loaded:', src);
        resolve(script);
      };
      
      script.onerror = (error) => {
        console.error('Script failed to load:', src, error);
        reject(error);
      };
      
      if (inline) {
        script.innerHTML = src;
        document.body.appendChild(script);
        resolve(script); // Inline scripts resolve immediately
      } else {
        script.src = src;
        document.body.appendChild(script);
      }
    });
  }

  function sf_chat_init() {
    var sf_scripts = [];
    var sf_scripts_index = -1;

    console.log('sf_chat_init');
    var scriptsrcs = document.getElementById('sf-chat-scripts').value;
    console.log('scriptsrcs:', scriptsrcs);

    if (scriptsrcs == null || scriptsrcs == undefined || scriptsrcs.trim().length <= 0) return;

    var arr = scriptsrcs.split('|');

    sf_scripts = arr.filter(function (value) {
      return value != null && value != '' && value.trim().length > 0;
    });

    console.log('sf_scripts:', sf_scripts);

    if (sf_scripts && sf_scripts.length > 0) sf_chat_load_scripts_async();

    async function sf_chat_load_scripts_async() {
      try {
        console.log('Loading scripts asynchronously...');
        
        // Carrega todos os scripts em paralelo
        const scriptPromises = sf_scripts.map(scriptSrc => {
          console.log('Starting to load script:', scriptSrc);
          return injectJSAsync(scriptSrc);
        });

        // Aguarda todos os scripts carregarem
        await Promise.all(scriptPromises);
        
        console.log('All scripts loaded successfully');
        waitForHelpButtonEnabled();
        
      } catch (error) {
        console.error('Error loading scripts:', error);
        // Em caso de erro, ainda tenta aguardar o elemento helpButtonEnabled
        // mas com um timeout menor para não deixar o usuário esperando muito
        waitForHelpButtonEnabled();
      }
    }

    function waitForHelpButtonEnabled() {
      let attempts = 0;
      const maxAttempts = 100;

      function checkElement() {
        const element = document.querySelector('.helpButtonEnabled');
        if (element) {
          document.getElementById('lixil-sf-bubble').classList.remove('chatbot-loading');
          return;
        }

        attempts++;
        if (attempts < maxAttempts) {
          setTimeout(checkElement, 200);
        } else {
          document.getElementById('lixil-sf-bubble').classList.remove('chatbot-loading');
          document.getElementById('lixil-sf-bubble').classList.add('hidden');
        }
      }

      checkElement();
    }

    
  }

  document.addEventListener('DOMContentLoaded', (event) => {
    console.log('page is fully loaded');
    //Use jquery from parent page if chatbubble loaded inside iframe.
    if (typeof jQuery == 'undefined' && window.location !== window.parent.location) {
      console.log('Jquery not loaded, using parent Jquery');
      window.jQuery = function (selector) {
        return parent.jQuery(selector, document);
      };
      jQuery = parent.$.extend(jQuery, parent.$);
      window.$ = jQuery;
      sf_chat_init();
    } else if (typeof jQuery == 'undefined') {
      console.log('Jquery not loaded');
      //Load Jquery manually, only needed if we open the chatbubble source page in browser
      injectJS('https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js', false, sf_chat_init);
    } else sf_chat_init();
  });
</script>


<script>
  window.bvCallback = () => BV.configure('global', {
    siteId: 'main_site',
    locale: 'en_US'
  });
</script>

