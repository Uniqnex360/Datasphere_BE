  </localization-form>


        


<script>
    class LocalizationForm extends HTMLElement {
        constructor() {
            super();
            this.elements = {
                input: this.querySelector('input[name="language_code"], input[name="country_code"]'),
                button: this.querySelector('button'),
                panel: this.querySelector('ul'),
            };
            this.elements.button.addEventListener('click', this.openSelector.bind(this));
            this.elements.button.addEventListener('focusout', this.closeSelector.bind(this));
            this.addEventListener('keyup', this.onContainerKeyUp.bind(this));

            window.addEventListener('scroll', this.closeOnScroll.bind(this), { passive: true });

            this.querySelectorAll('a').forEach(item => item.addEventListener('click', this.onItemClick.bind(this)));
        }

        hidePanel() {
            this.elements.button.setAttribute('aria-expanded', 'false');
            this.elements.panel.setAttribute('hidden', true);
        }

        closeOnScroll() {
            if (!this.elements.panel.hasAttribute('hidden')) {
                this.hidePanel();
            }
        }

        onContainerKeyUp(event) {
            if (event.code.toUpperCase() !== 'ESCAPE') return;
            this.hidePanel();
            this.elements.button.focus();
        }

        onItemClick(event) {
            event.preventDefault();
            const form = this.querySelector('form');
            const clickedValue = event.currentTarget.getAttribute('data-value');
            const inputLanguage = this.querySelector('input[name="language_code"]');
            const inputCountry = this.querySelector('input[name="country_code"]');

            if (inputCountry && event.currentTarget.hasAttribute('data-country')) {
                inputCountry.value = clickedValue;
                if (inputLanguage) inputLanguage.value = 'en';
            }
            else if (inputLanguage) {
                inputLanguage.value = clickedValue;
            }

            this.clearProductCompareData();

            if (form) form.submit();
        }

        openSelector() {
            this.elements.button.focus();
            this.elements.panel.toggleAttribute('hidden');
            this.elements.button.setAttribute('aria-expanded', (this.elements.button.getAttribute('aria-expanded') === 'false').toString());
        }

        closeSelector(event) {
            const shouldClose = event.relatedTarget && event.relatedTarget.nodeName === 'BUTTON';
            if (event.relatedTarget === null || shouldClose) {
                this.hidePanel();
            }
        }
        
        /**
         * Limpa dados de comparação de produtos no localStorage
         */
        clearProductCompareData() {
            try {
                // Remove os itens específicos do localStorage
                localStorage.removeItem('compareProducts');
                localStorage.removeItem('compareSpecificationTable');
                
                // Procura por outros itens relacionados a produtos que possam existir
                const productKeysToRemove = [];
                
                // Itera por todas as chaves no localStorage
                for (let i = 0; i < localStorage.length; i++) {
                    const key = localStorage.key(i);
                    
                    // Verifica se a chave está relacionada a produtos
                    if (key && (
                        key.includes('product') || 
                        key.includes('compare') || 
                        key.includes('cart') || 
                        key.includes('recently_viewed')
                    )) {
                        productKeysToRemove.push(key);
                    }
                }
                
                // Remove as chaves encontradas
                productKeysToRemove.forEach(key => {
                    localStorage.removeItem(key);
                });
                
                console.log('Dados de comparação de produtos limpos devido à mudança de idioma');
            } catch (error) {
                console.error('Erro ao limpar dados de comparação de produtos:', error);
            }
        }
    }

    customElements.define('localization-form', LocalizationForm);
</script>
      </div>
      
