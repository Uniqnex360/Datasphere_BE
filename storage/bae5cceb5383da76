<script>
    const raw = "";
  
    const pattern = /\[\(([^)\]]+)\)[|:](.+?)\]/g;
  
    const categoriesMap = {};
  
    for (const match of raw.matchAll(pattern)) {
      const categoryList = match[1].split(',').map(str => str.trim());
      const colorList = match[2].split(',').map(str => str.trim()).join(',');
  
      for (const category of categoryList) {
        categoriesMap[category] = colorList;
      }
    }
  
  
    window.theme.swatchesCategory = categoriesMap;
  
