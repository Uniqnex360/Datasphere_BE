<script>

  function getVariantIdFromUrl() {
    const params = new URLSearchParams(window.location.search);
    return params.get('variant');
  }

  function updatePriceByVariant() {
    const currentVariantId = getVariantIdFromUrl();
    if (!currentVariantId) return;

    document.querySelectorAll('[data-price-list]').forEach(el => {
      if (el.getAttribute('data-price-list') === currentVariantId) {
        el.classList.remove('hidden');
      } else {
        el.classList.add('hidden');
      }
    });
  }

  document.addEventListener('DOMContentLoaded', updatePriceByVariant);
  window.addEventListener('popstate', updatePriceByVariant);
  ['pushState', 'replaceState'].forEach(method => {
    const orig = history[method];
    history[method] = function() {
      const result = orig.apply(this, arguments);
      updatePriceByVariant();
      return result;
    };
  });
</script>
        
     
      
      
        
      
        
      
        
      
        
      
