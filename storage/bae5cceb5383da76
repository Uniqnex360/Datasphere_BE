            VariantSelectorBtnText: "",
            wishlistTitle: "",
            wishlistInfo: "",
            emptyWishlistTitle: "",
            emptyWishlistDescription: "",
            
            collectionTitle: "",
            emptyCarouselCollectionText: "",
            emptyCollectionText: "",
            emptyCollectionDescription: "",
            
            addToCollectionTitle: "",
            removeItemCta: "",
            addToCollectionCta: "",
            createCollectionCta: "",
            saveNewCollectionCta: "",
            renameCollectionCta: "",
            deleteCollectionCta: "",
            shareCollectionCta: "",
            saveCollectionCta: "",
