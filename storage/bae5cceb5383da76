 () =\u003e {\n  updateFormStatus(); \n  handleAddressValidation();\n});\n}\n\nfunction listenToAddressValidationEvents() {\nconsole.log('listenToAddressValidationEvents');\nwindow.addEventListener('message', function (event) {\n  const { type, data } = event.data;\n  const events = getEvents();\n  events[type]?.(data);\n});\n}\n\nfunction getEvents() {\nreturn {\n  \"disableForm\": handleDisableForm,\n  \"enableForm\": handleEnableForm,\n  \"addressValidated\": handleAddressValidated,\n  \"selectTopicByUrl\": handleSelectTopicByUrl\n}\n}\n\nfunction handleDisableForm() {\nconst form = document.querySelector('form');\nArray.from(form.elements).forEach((element) =\u003e {\n  element.disabled = true;\n});\n}\n\nfunction handleEnableForm() {\nconst form = document.querySelector('form');\nArray.from(form.elements).forEach((element) =\u003e {\n  element.disabled = false;\n});\n}\n\nfunction handleAddressValidated(data) {\nconsole.log(\"handleAddressValidated\", data);\nconst { addressInput, addressInput2, cityInput, provinceInput, zipInput, countryInput } = getAddressFormFields();\n\nconsole.log(\"Setting address fields with addressInput:\", data.streetLinesToken[0]);\n\naddressInput.value = data.streetLinesToken[0];\nif(data.streetLinesToken[1]) {\n  addressInput2.value = data.streetLinesToken[1];\n}\ncityInput.value = data.city;\nprovinceInput.value = data.stateOrProvinceCode;\nzipInput.value = data.postalCode;\ncountryInput.value = data.countryCode || 'US';\n\naddressFormStatus.validated = true;\naddressFormStatus.validateOnNextFocusOut = false;\n}\n\nfunction getFormFields() {\nreturn {\n  firstName: document.getElementById('first_name'),\n  lastName: document.getElementById('last_name'),\n  phone: document.getElementById('phone'),\n  email: document.getElementById('email'),\n  orderNumber: document.getElementById('order_number'),\n  productModelNumber: document.getElementById('product_model_number'),\n  purchaseDate: document.getElementById('purchase_date'),\n  mess