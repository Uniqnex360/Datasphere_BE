            function getCountryCode() {
                if(thaliacustomercountry == ''){ 
                    thaliaCustomerCookie();
                    thaliacustomercountry = thaliaGetCookie("thaliacustomercountry");
                     getCName(thaliacustomercountry);
                     }else{
