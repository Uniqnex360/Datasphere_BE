    window._RestockRocketConfig.variantsShippingText = {29021586247 : "",29021586375 : "",29021586503 : "",29021586631 : "",29021586759 : "",29021586887 : "",29021586951 : "",29021585223 : "",29021585479 : "",29021585607 : "",29021585799 : "",29021585991 : "",29021586119 : "",};
