jQuery.fn.outerHTML = function() {											
return jQuery('<div />').append(this.eq(0).clone()).html();								
};																							
var ids="";																				
var id ="";																				
var sku ="";																				
var catalogid ="";																		
var pos=0;																					
var total=0;																				
var currency="";																			
var content_type="";																		
var hasAdvancedOptions=false;																
jQuery(":button").each(function() {														if(jQuery(this).outerHTML().indexOf('check_and_add(')!=-1)  {														
		if(pos>100)																			
			return false;																	
		var target = jQuery(this);															
		if(target.data().relHook) {															
			if(jQuery("#product_id").html()!='undefined' && jQuery("#product_id").html()!=undefined && jQuery("#product_id").html()!='')		
				sku = jQuery("#product_id").html();										
			else																			
				sku = target.data().relHookdataId;											
			catalogid = target.data().relHookdataCatalogid;									
			var array1 = "inventoryarray"+catalogid;										
			try{																			
				var array2 = eval(array1);													
				if ((typeof array2 == 'object') && (array2 instanceof Array == true) && (array2.length>0)){			
					array1 = "idarray"+catalogid;											
				    array2 = eval(array1);													
					if ((typeof array2 == 'object') && (array2 instanceof Array == true) && (array2.length>0)){		
						hasAdvancedOptions=true;											
					}																		
					else																	
					{																		
						hasAdvancedOptions=false;											
					}																		
				}																			
				else																		
				{																			
					hasAdvancedOptions=false;												
				}																			
			}																				
			catch(e){																		
				if (e.name.toString() == 'ReferenceError' ){								
					hasAdvancedOptions=false;												
				}																			
			}																				
			if(hasAdvancedOptions){															
				id=target.data().relHookdataId;												
				content_type="product_group"												
			}																				
			else{																			
				id=sku;																		
				content_type="product"													
			}																				
			if(pos==0){																		
				ids="'"+id+"'";															
				currency = target.data().relHookdataCurrency;								
			}																				
			else{																			
				ids=ids+","+"'"+id+"'";												
			}																				
			var price = target.data().relHookdataPrice;										
			total+=parseFloat(price);														
			pos = pos + 1;																	
		}																					
	}																						
});																						
total = total.toFixed(2);																	
if(ids!=""){																				
	ids = "["+ids+"]";																	
	fbq('track', 'ViewContent', {															
		content_ids: ids,																	
		content_type: content_type,															
		value: total,																		
		currency: currency																	
	});																						
}																																											
</script>																					
