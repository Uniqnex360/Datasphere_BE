<script>
  var CaptchaCallback = function () {
    jQuery('.recaptchaRobot').each(function (index, element) {
      var id = jQuery(element).attr('id');
      var widgetId = '';
      widgetId = grecaptcha.render(id);
      jQuery(element).attr('data-widgetId', widgetId);
    });
  };
</script>
