                                redirect: 'follow'
                                };

                                fetch("/rest/all/V1/subscriber", requestOptions)
                                .then(response => response.text())
                                .then(result => console.log("newsletter subscription",result))
                                .catch(error => console.log('error', error));
                            }
                    })
                        }
                        
                       
                    }, 3000)

                }
                document.querySelectorAll('.anchor-continue').forEach(closeItem => {
                    closeItem.addEventListener('click',()=>{
                        setCsCookie('prompt-not-show',true,1)
                    })
                })
                
        }
        else{
            document.getElementById('captured-email').classList.add('cookie-is-present');
        }


    })
    function validateEmail(email){
        let data = /^(([^<>()[\]\.,;:\s@\"]+(\.[^<>()[\]\.,;:\s@\"]+)*)|(\".+\"))@(([^<>()[\]\.,;:\s@\"]+\.)+[^<>()[\]\.,;:\s@\"]{3,})$/i;
        return data.test(email)
    }
    function getCsCookie(name) {
     var name = name + "=";
     var ca = document.cookie.split(";");
     for (var i = 0; i < ca.length; i++) {
         var c = ca[i];
         while (c.charAt(0) == " ") {
             c = c.substring(1);
         }
         if (c.indexOf(name) == 0) {
             return c.substring(name.length, c.length);
         }
     }
     return "";
 }
 function setCsCookie(name, value, days = 1) {
        var d = new Date();
        d.setTime(d.getTime() + days * 24 * 60 * 60 * 1000);
        var expires = "expires=" + d.toUTCString();
        document.cookie = name + "=" + value + ";" + expires + ";path=/";
    }
