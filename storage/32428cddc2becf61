<script>
  /* MOBILE ONLY: inject our three sections & hide native menu tree */
  (function () {
    const drawerId = 'MenuDrawer';
    const tplId = 'mm-sections-template-sections--25724303245633__header';
    const isMobile = () => window.matchMedia('(max-width:1023px)').matches;

    function getScrollHost(drawer) {
      return drawer.querySelector('.drawer__scrollable, .menu-drawer__scrollable, .menu-drawer__content') || drawer;
    }

    function hideNativeMenu(drawer) {
      if (!isMobile()) return;
      const host = getScrollHost(drawer);
      const selectors = [
        ':scope nav',
        ':scope .menu-drawer__navigation',
        ':scope .menu-drawer__menu',
        ':scope .menu-drawer__main',
        ':scope .menu-drawer__list',
        ':scope .menu-drawer__primary',
        ':scope .menu-drawer__secondary',
      ];
      selectors.forEach((sel) => {
        host.querySelectorAll(sel).forEach((el) => {
          if (!el.closest('.mm-sections')) el.classList.add('mm-hide-mobile');
        });
      });
    }

    function ensureSectionsInjected() {
      const drawer = document.getElementById(drawerId);
      const tpl = document.getElementById(tplId);
      if (!drawer || !tpl) return;

      const host = getScrollHost(drawer);

      if (isMobile() && !host.querySelector('.mm-sections')) {
        const clone = tpl.content.firstElementChild.cloneNode(true);
        host.prepend(clone);
      }

      if (isMobile()) {
        hideNativeMenu(drawer);
        host.style.overflow = host.style.overflow || 'auto';
        host.style.maxHeight = host.style.maxHeight || '100dvh';
        host.style.webkitOverflowScrolling = 'touch';
      } else {
        drawer.querySelectorAll('.mm-hide-mobile').forEach((el) => el.classList.remove('mm-hide-mobile'));
        host.style.overflow = '';
        host.style.maxHeight = '';
        host.style.webkitOverflowScrolling = '';
      }
    }

    document.addEventListener('click', (e) => {
      const btn = e.target.closest('[aria-controls="MenuDrawer"], .menu-drawer-button');
      if (!btn) return;
      requestAnimationFrame(() => setTimeout(ensureSectionsInjected, 0));
    });
    document.addEventListener('DOMContentLoaded', ensureSectionsInjected);
    window.addEventListener('resize', ensureSectionsInjected);
  })();
</script>

<script type="application/ld+json">
  {
    "@context": "http://schema.org",
