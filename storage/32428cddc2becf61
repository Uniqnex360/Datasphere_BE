  .mega-menu__promotions .card-media--link .card-media {border-radius: 0px;width:100%;}
  .mega-menu__promotions .card-media--link{width:100%;}
  .mega-menu__promotions .mega-menu__promotions--single img{border-radius:0;}
 
}
</style>

