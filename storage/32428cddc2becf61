      "https:\/\/pin.it\/2BitLz9Hj",
      "https:\/\/www.instagram.com\/bestbuyfurniture\/?hl=en",
      "https:\/\/www.tiktok.com\/@bestbuyfurniture",
      "",
      "",
      "https:\/\/www.youtube.com\/user\/bestbuyfurniture",
      ""
    ],
    "url": "https:\/\/bestbuy-furniture.com"
  }
</script><style>
  @media (min-width:1024px){
    .inline-flex:hover details[is="details-dropdown"] { display: block; }
    .inline-flex:hover .dropdown { opacity: 1; visibility: visible; }
  }
</style>
<script>
  if (window.matchMedia('(min-width:1024px)').matches) {
    document.querySelectorAll('.inline-flex.items-center').forEach((item) => {
      const details = item.querySelector('details');
      const dropdown = item.querySelector('.dropdown');
      const megaMenu = item.querySelector('.mega-menu');
      const megaMenuContainer = item.querySelector('.mega-menu__container');

      item.addEventListener('mouseenter', () => {
        document.body.classList.add('has-dropdown-menu');
        if (details) details.setAttribute('open', '');
        if (dropdown) dropdown.setAttribute('open', '');
        if (megaMenu) {
          megaMenu.setAttribute('open', '');
          megaMenu.classList.add('custom-open-megamenu');
        }
        if (megaMenuContainer) {
          megaMenuContainer.style.visibility = 'visible';
          megaMenuContainer.style.transform = 'translateY(0px)';
          megaMenuContainer.style.opacity = '1';
        }
      });

      item.addEventListener('mouseleave', () => {
        if (![...document.querySelectorAll('.inline-flex.items-center:hover')].length) {
          document.body.classList.remove('has-dropdown-menu');
        }
        if (details) details.removeAttribute('open');
        if (dropdown) dropdown.removeAttribute('open');
        if (megaMenu) {
          megaMenu.removeAttribute('open');
          megaMenu.classList.remove('custom-open-megamenu');
        }
        if (megaMenuContainer) {
          megaMenuContainer.style.visibility = 'hidden';
          megaMenuContainer.style.transform = 'translateY(-20px)';
          megaMenuContainer.style.opacity = '0';
        }
      });
    });
  }
</script>

