<script>
  (function() {
    var sectionEl = document.querySelector('.section-sections--25724303245633__affirm_financing_bar_tjQXdd.tm-affirm-announcement');
    if (!sectionEl) return;

    var root = sectionEl.querySelector('.tm-affirm-slider');
    if (root) {
      var slides = root.querySelectorAll('.tm-affirm-slide');
      if (slides.length > 1) {
        var index = 0;
        var delay = 6000;
        if (!delay || delay < 1000) delay = 5000;

        function showSlide(i) {
          slides[index].classList.remove('is-active');
          index = i;
          slides[index].classList.add('is-active');
        }

        setInterval(function() {
          var next = (index + 1) % slides.length;
          showSlide(next);
        }, delay);
      }
    }

    var closeBtn = sectionEl.querySelector('.tm-affirm-close');
    if (closeBtn) {
      closeBtn.addEventListener('click', function() {
        sectionEl.style.display = 'none';
      });
    }
  })();
</script>


