<script>
document.querySelectorAll('[data-cart-item-add]').forEach(form => form.addEventListener('submit', (event) => {
  event.preventDefault();
  const formData = new FormData(event.target);
  let productId, productQty;
  for (const pair of formData.entries()) {
    if (pair[0] === 'product_id') {
      productId = pair[1];
    } else if (pair[0] === 'qty[]') {
      productQty = parseInt(pair[1]);
    }
  }
  const product = {
      item_id: 'BC-HOME-MXD',
      item_sku: 'BC-HOME-MXD',
      item_name: 'HOME Emergency Medical Alert System',
      quantity: productQty,
      item_brand: 'Medical Care Alert',
      item_category: 'In-Home Medical Alert Systems',
      item_variant: '',
      currency: 'USD',
      price: 29.95
  }
window.dataLayer.push({
  event: 'Ecomm_add_to_cart',  // name of the event. In this case, it always must be add_to_cart
  ecommerce: {
    items: [product],
    currency: product.currency,
    value: product.price   
  }
});                                                     
}));
</script>

