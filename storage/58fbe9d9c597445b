    <!-- Reputation gate + conditional GTM loader -->
<script
  data-reputable-key="rdb_pub_6RPFOirw3e_RoxDdDgYKy1A7XZ_wzH80"
  data-reputable-endpoint="https://finditparts-test.verify.reputable.click"
  data-reputable-path="/_reputable/reputation"
  data-reputable-consent="auto"
  data-reputable-storage="session"
  data-reputable-timeout="15000"
  data-reputable-cache-ttl="3600"
  data-reputable-cache-storage="local"
  data-reputable-track-clicks="false"
  data-gtm-id="GTM-MH9RRJ"
>
(function () {
  'use strict';

  // ---------------------------
  // Safe helpers (never throw)
  // ---------------------------
  var scriptEl = document.currentScript;

  function getAttr(name, fallback) {
    try {
      if (scriptEl && scriptEl.getAttribute) {
        var v = scriptEl.getAttribute(name);
        if (v !== null && v !== undefined && v !== '') return v;
      }
    } catch (e) {}
    return fallback;
  }

  function now() {
    try { return Date.now ? Date.now() : new Date().getTime(); } catch (e) { return 0; }
  }

  function safeJsonParse(text) {
    try { return JSON.parse(text); } catch (e) { return null; }
  }

  function hasOwn(obj, key) {
    try { return Object.prototype.hasOwnProperty.call(obj, key); } catch (e) { return false; }
  }

  function schedule(fn) {
    try {
      if (typeof requestIdleCallback === 'function') {
        requestIdleCallback(function () { try { fn(); } catch (e) {} }, { timeout: 1000 });
        return;
      }
    } catch (e) {}
    setTimeout(function () { try { fn(); } catch (e2) {} }, 0);
  }

  function dispatchDecision(detail) {
    try {
      if (typeof window.CustomEvent === 'function') {
        window.dispatchEvent(new CustomEvent('reputable:decision', { detail: detail }));
        return;
      }
      if (document.createEvent) {
        var evt = document.createEvent('CustomEvent');
        evt.initCustomEvent('reputable:decision', false, false, detail);
        window.dispatchEvent(evt);
      }
    } catch (e) {}
  }

  // ---------------------------
  // Config
  // ---------------------------
  var clientKey = getAttr('data-reputable-key', '');
  var endpointAttr = String(getAttr('data-reputable-endpoint', '') || '').trim();
  var pathBaseAttr = String(getAttr('data-reputable-path', '/_reputable/reputation') || '').trim();

  var consentMode = String(getAttr('data-reputable-consent', 'auto')).toLowerCase();
  var requestedStorage = String(getAttr('data-reputable-storage', 'session')).toLowerCase();

  var decisionTimeoutMs = parseInt(getAttr('data-reputable-timeout', '500'), 10);
  if (!isFinite(decisionTimeoutMs) || decisionTimeoutMs < 0) decisionTimeoutMs = 500;

  var cacheTtlSeconds = parseInt(getAttr('data-reputable-cache-ttl', '3600'), 10);
  if (!isFinite(cacheTtlSeconds) || cacheTtlSeconds < 0) cacheTtlSeconds = 3600;

  var cacheStorageMode = String(getAttr('data-reputable-cache-storage', 'local')).toLowerCase();
  var trackClicks = String(getAttr('data-reputable-track-clicks', 'false')).toLowerCase() === 'true';

  var gtmId = getAttr('data-gtm-id', '');

  window.dataLayer = window.dataLayer || [];

  // ---------------------------
  // Google Tag Manager loader (wrapped)
  // ---------------------------
  function loadGTM() {
    if (!gtmId) return;
    if (loadGTM._loaded) return;
    loadGTM._loaded = true;

    try {
      window.dataLayer.push({ 'gtm.start': now(), event: 'gtm.js' });
    } catch (e) {}

    try {
      var d = document;
      var firstScript = d.getElementsByTagName('script')[0];
      var j = d.createElement('script');
      j.async = true;
      j.src =
        'https://www.googletagmanager.com/gtm.js?id=' +
        encodeURIComponent(gtmId) +
        '&l=dataLayer';

      if (firstScript && firstScript.parentNode) {
        firstScript.parentNode.insertBefore(j, firstScript);
      } else if (d.head) {
        d.head.appendChild(j);
      } else {
        d.documentElement.appendChild(j);
      }
    } catch (e2) {}
  }

  // ---------------------------
  // Privacy / consent logic
  // ---------------------------
  function getTimeZone() {
    try {
      if (window.Intl && Intl.DateTimeFormat) {
        return Intl.DateTimeFormat().resolvedOptions().timeZone || '';
      }
    } catch (e) {}
    return '';
  }

  function hasPrivacySignal() {
    try { if (navigator && navigator.globalPrivacyControl === true) return true; } catch (e) {}
    try {
      var dnt = String((navigator && navigator.doNotTrack) || '').toLowerCase();
      if (dnt === '1' || dnt === 'yes') return true;
    } catch (e2) {}
    try { if (getTimeZone().indexOf('Europe/') === 0) return true; } catch (e3) {}
    return false;
  }

  var consentResolved =
    consentMode === 'granted'
      ? true
      : consentMode === 'denied'
        ? false
        : !hasPrivacySignal();

  // ---------------------------
  // Endpoints
  // - origin from data-reputable-endpoint (if provided), otherwise same-origin
  // - base path from data-reputable-path, default /_reputable/reputation
  // ---------------------------
  function stripToOrigin(raw) {
    // URL-based (no regex) to avoid escaping issues in templates/build steps
    try {
      return new URL(String(raw || ''), window.location.href).origin;
    } catch (e) {
      return '';
    }
  }

  function normalizeBasePath(p, fallback) {
    var s = String(p || '').trim();
    if (!s) s = fallback;
    if (s.charAt(0) !== '/') s = '/' + s;
    while (s.length > 1 && s.charAt(s.length - 1) === '/') s = s.slice(0, -1);
    return s;
  }

  var origin = stripToOrigin(endpointAttr); // "" => same-origin
  var basePath = normalizeBasePath(pathBaseAttr, '/_reputable/reputation');

  var decisionEndpoint = (origin ? origin : '') + basePath + '/';
  var behaviorEndpoint = (origin ? origin : '') + basePath + '/b';

  // ---------------------------
  // Session persistence (sid)
  // ---------------------------
  var ALLOWED_COUNTRIES = ['US'];
  var detectedCountry = null;

  function canPersistSession() {
    if (consentMode === 'granted') return true;
    if (consentMode === 'denied') return false;

    if (detectedCountry) {
      var cc = String(detectedCountry).toUpperCase();
      if (ALLOWED_COUNTRIES.indexOf(cc) === -1) return false;
    }
    return !hasPrivacySignal();
  }

  function getCookie(name) {
    try {
      var parts = String(document.cookie || '').split(';');
      for (var i = 0; i < parts.length; i++) {
        var p = parts[i].trim().split('=');
        var k = p.shift();
        if (k === name) return decodeURIComponent(p.join('=') || '');
      }
    } catch (e) {}
    return null;
  }

  function setCookie(name, value, maxAgeSeconds) {
    try {
      var base = name + '=' + encodeURIComponent(value) + '; Path=/; SameSite=Lax';
      var maxAge = (typeof maxAgeSeconds === 'number') ? '; Max-Age=' + String(maxAgeSeconds) : '';
      document.cookie = base + maxAge;
    } catch (e) {}
  }

  function deleteCookie(name) {
    try { setCookie(name, '', 0); } catch (e) {}
  }

  function getStorage(mode) {
    try {
      if (mode === 'local') return window.localStorage;
      if (mode === 'session') return window.sessionStorage;
    } catch (e) {}
    return null;
  }

  var sidStorageMode =
    requestedStorage === 'local' || requestedStorage === 'cookie'
      ? requestedStorage
      : 'session';

  var SESSION_KEY = 'reputable_sid';
  var memorySessionId = null;

  function getSessionId() {
    if (memorySessionId) return memorySessionId;
    if (!canPersistSession()) return null;

    var store = getStorage(sidStorageMode);
    if (store) {
      try { memorySessionId = store.getItem(SESSION_KEY); } catch (e) {}
      return memorySessionId;
    }
    memorySessionId = getCookie(SESSION_KEY);
    return memorySessionId;
  }

  function setSessionId(sid, country) {
    if (!sid) return;

    if (country) detectedCountry = country;
    memorySessionId = sid;

    if (!canPersistSession()) return;

    var store = getStorage(sidStorageMode);
    if (store) {
      try { store.setItem(SESSION_KEY, sid); } catch (e) {}
      return;
    }
    setCookie(SESSION_KEY, sid, 60 * 60 * 24 * 30);
  }

  // ---------------------------
  // Decision cache (outcome + ignore_analytics) with expiry
  // - ONLY stored/used if ignoreAnalytics is boolean
  // ---------------------------
  var DECISION_CACHE_KEY = 'reputable_decision_v1:' + (clientKey || 'default');

  function canPersistCache() {
    if (consentMode === 'granted') return true;
    if (consentMode === 'denied') return false;
    return !hasPrivacySignal();
  }

  function getCacheStore() {
    if (cacheStorageMode === 'cookie') return null;
    if (cacheStorageMode === 'session') return getStorage('session');
    return getStorage('local');
  }

  function cacheGetRaw() {
    if (!canPersistCache()) return null;
    var store = getCacheStore();
    if (store) {
      try { return store.getItem(DECISION_CACHE_KEY); } catch (e) { return null; }
    }
    return getCookie(DECISION_CACHE_KEY);
  }

  function cacheSetRaw(value, ttlSeconds) {
    if (!canPersistCache()) return false;

    var store = getCacheStore();
    if (store) {
      try { store.setItem(DECISION_CACHE_KEY, value); return true; } catch (e) { return false; }
    }
    setCookie(DECISION_CACHE_KEY, value, ttlSeconds);
    return true;
  }

  function cacheDelete() {
    try {
      var store = getCacheStore();
      if (store) {
        try { store.removeItem(DECISION_CACHE_KEY); } catch (e) {}
      } else {
        deleteCookie(DECISION_CACHE_KEY);
      }
    } catch (e2) {}
  }

  function readDecisionCache() {
    try {
      var raw = cacheGetRaw();
      if (!raw) return null;

      var obj = safeJsonParse(raw);
      if (!obj || obj.v !== 1) return null;
      if (typeof obj.ignoreAnalytics !== 'boolean') return null;

      if (typeof obj.exp !== 'number' || obj.exp <= now()) {
        cacheDelete();
        return null;
      }

      return obj;
    } catch (e) {
      return null;
    }
  }

  function writeDecisionCache(ignoreAnalytics, outcome, country) {
    try {
      if (!canPersistCache()) return;
      if (typeof ignoreAnalytics !== 'boolean') return;
      if (!isFinite(cacheTtlSeconds) || cacheTtlSeconds <= 0) return;

      var t = now();
      var record = {
        v: 1,
        set: t,
        exp: t + (cacheTtlSeconds * 1000),
        ignoreAnalytics: ignoreAnalytics,
        outcome: (typeof outcome === 'string' ? outcome : null),
        country: (typeof country === 'string' ? country : null)
      };

      cacheSetRaw(JSON.stringify(record), cacheTtlSeconds);
    } catch (e) {}
  }

  // ---------------------------
  // Decision payload
  // ---------------------------
  function buildPayload() {
    var loc = window.location;
    return {
      client_key: clientKey,
      consent: consentMode,
      sid: getSessionId(),

      page_url: (loc && loc.href) ? loc.href : null,
      page_path: (loc && loc.pathname) ? loc.pathname : null,
      page_query: (loc && loc.search) ? loc.search : '',

      referrer: (document && document.referrer) ? document.referrer : null,
      page_title: (document && document.title) ? document.title : '',

      tz: getTimeZone(),
      lang: (navigator && navigator.language) ? navigator.language : '',

      screen: (window.screen && window.screen.width && window.screen.height)
        ? (window.screen.width + 'x' + window.screen.height)
        : ''
    };
  }

  // ---------------------------
  // Network (fetch with XHR fallback)
  // ---------------------------
  function postJson(url, payload, callback) {
    var called = false;
    function done(err, json, status) {
      if (called) return;
      called = true;
      try { callback(err || null, json || null, status || 0); } catch (e) {}
    }

    if (typeof window.fetch === 'function') {
      try {
        window.fetch(url, {
          method: 'POST',
          headers: { 'Content-Type': 'application/json' },
          body: JSON.stringify(payload),
          mode: 'cors',
          credentials: 'omit'
        })
          .then(function (res) {
            res.text()
              .then(function (text) { done(null, safeJsonParse(text), res.status); })
              .catch(function () { done(null, null, res.status); });
          })
          .catch(function (err) { done(err, null, 0); });
      } catch (e2) {
        done(e2, null, 0);
      }
      return;
    }

    try {
      var xhr = new XMLHttpRequest();
      xhr.open('POST', url, true);
      xhr.setRequestHeader('Content-Type', 'application/json');
      xhr.onreadystatechange = function () {
        if (xhr.readyState !== 4) return;
        done(null, safeJsonParse(xhr.responseText), xhr.status || 0);
      };
      xhr.onerror = function () { done(new Error('Network error'), null, 0); };
      xhr.send(JSON.stringify(payload));
    } catch (e3) {
      done(e3, null, 0);
    }
  }

  // ---------------------------
  // Response parsing
  // ---------------------------
  function extractIgnoreAnalytics(respObj) {
    if (!respObj) return null;

    if (hasOwn(respObj, 'ignore_analytics')) {
      return (respObj.ignore_analytics === true || respObj.ignore_analytics === false)
        ? respObj.ignore_analytics
        : null;
    }
    if (hasOwn(respObj, 'ignoreAnalytics')) {
      return (respObj.ignoreAnalytics === true || respObj.ignoreAnalytics === false)
        ? respObj.ignoreAnalytics
        : null;
    }
    return null;
  }

  function normalizeDecision(json) {
    var r = (json && json.response) ? json.response : json;
    return {
      sessionId: (r && r.sessionId) ? r.sessionId : null,
      outcome: (r && typeof r.outcome === 'string') ? r.outcome : null,
      country: (r && typeof r.country === 'string') ? r.country : null,
      ignoreAnalytics: extractIgnoreAnalytics(r),
      raw: json || null
    };
  }

  // ---------------------------
  // Optional behavior tracking
  // ---------------------------
  var behaviorStarted = false;
  var mouseMovements = 0;
  var lastMouseTime = 0;
  var clickEvents = [];
  var scrollDepthMax = 0;
  var lastBehaviorSend = 0;

  var BEHAVIOR_INTERVAL_MS = 15000;
  var MAX_CLICKS_BUFFER = 10;

  function sendBeacon(url, obj) {
    try {
      if (!navigator || typeof navigator.sendBeacon !== 'function') return false;
      try {
        var blob = new Blob([JSON.stringify(obj)], { type: 'application/json' });
        return navigator.sendBeacon(url, blob);
      } catch (e) {
        return navigator.sendBeacon(url, JSON.stringify(obj));
      }
    } catch (e2) {
      return false;
    }
  }

  function sendBehavior(eventType, data) {
    var sid = getSessionId();
    if (!sid) return;

    var payload = {
      type: 'behavior',
      event: eventType,
      sid: sid,
      client_key: clientKey,
      page_path: (window.location && window.location.pathname) ? window.location.pathname : null,
      data: data || {}
    };

    sendBeacon(behaviorEndpoint, payload);
  }

  function trackMouseMove() {
    var t = now();
    if (t - lastMouseTime < 100) return;
    lastMouseTime = t;
    mouseMovements++;
  }

  function trackScrollDepth() {
    try {
      var scrollTop = window.scrollY || (document.documentElement && document.documentElement.scrollTop) || 0;
      var docHeight =
        Math.max(
          document.body ? document.body.scrollHeight : 0,
          document.documentElement ? document.documentElement.scrollHeight : 0
        ) - (window.innerHeight || 0);

      var depth = docHeight > 0 ? Math.round((scrollTop / docHeight) * 100) : 0;
      if (depth > scrollDepthMax) scrollDepthMax = depth;
    } catch (e) {}
  }

  function getClickTarget(el) {
    if (!el || !el.tagName) return null;

    var tag = String(el.tagName).toLowerCase();
    var id = el.id ? ('#' + el.id) : '';

    var cls = '';
    try {
      if (el.className && typeof el.className === 'string') {
        cls = '.' + el.className.trim().split(/\s+/).slice(0, 2).join('.');
      }
    } catch (e) {}

    var text = '';
    try {
      if (tag === 'button' || tag === 'a') text = (el.textContent || '').trim().substring(0, 20);
    } catch (e2) {}

    return { tag: tag, id: id, cls: cls, text: text || undefined };
  }

  function trackClick(e) {
    if (!trackClicks) return;

    try {
      var target = getClickTarget(e.target);
      if (!target) return;

      clickEvents.push({
        t: now(),
        target: target,
        x: Math.round(e.clientX || 0),
        y: Math.round(e.clientY || 0)
      });

      if (clickEvents.length > MAX_CLICKS_BUFFER) clickEvents.shift();
    } catch (e) {}
  }

  function sendPeriodicBehavior() {
    var t = now();
    if (mouseMovements === 0 && clickEvents.length === 0) return;
    if (t - lastBehaviorSend < BEHAVIOR_INTERVAL_MS) return;

    lastBehaviorSend = t;

    sendBehavior('activity', {
      mouseMovements: mouseMovements,
      clicks: clickEvents.slice(),
      scrollDepth: scrollDepthMax
    });

    mouseMovements = 0;
    clickEvents = [];
  }

  var leaveSent = false;
  function sendLeaveEvent() {
    if (leaveSent) return;
    leaveSent = true;

    sendBehavior('leave', {
      scrollDepthMax: scrollDepthMax
    });
  }

  function startBehaviorTracking() {
    if (behaviorStarted) return;
    behaviorStarted = true;

    try {
      document.addEventListener('mousemove', trackMouseMove, { passive: true });
      window.addEventListener('scroll', trackScrollDepth, { passive: true });

      if (trackClicks) {
        document.addEventListener('click', trackClick, { passive: true, capture: true });
      }

      setInterval(sendPeriodicBehavior, 5000);

      document.addEventListener('visibilitychange', function () {
        try { if (document.visibilityState === 'hidden') sendLeaveEvent(); } catch (e) {}
      });

      window.addEventListener('pagehide', sendLeaveEvent, { passive: true });
      window.addEventListener('beforeunload', sendLeaveEvent, { passive: true });
    } catch (e) {}
  }

  // ---------------------------
  // Analytics gating (cache-first, fail-open)
  // ---------------------------
  var analyticsFinalized = false;

  function finalizeAnalytics(shouldLoad) {
    if (analyticsFinalized) return;
    analyticsFinalized = true;

    if (shouldLoad) loadGTM();
  }

  // Cache-first: if valid, decide immediately (no network; no delay)
  var cached = readDecisionCache();
  if (cached) {
    try {
      window.__REPUTABLE__ = {
        sessionId: getSessionId(),
        outcome: cached.outcome || null,
        country: cached.country || null,
        ignoreAnalytics: cached.ignoreAnalytics,
        raw: cached,
        canPersist: canPersistSession(),
        source: 'cache'
      };
      dispatchDecision(window.__REPUTABLE__);
    } catch (e) {}

    finalizeAnalytics(cached.ignoreAnalytics ? false : true);

    if (getSessionId()) schedule(startBehaviorTracking);
    return;
  }

  // Cache miss: allow a short window for decision; otherwise fail-open load GTM
  var gateTimer = setTimeout(function () {
    finalizeAnalytics(true);
  }, decisionTimeoutMs);

  schedule(runDecision);

  function runDecision() {
    var payload = buildPayload();

    postJson(decisionEndpoint, payload, function (err, json) {
      try { clearTimeout(gateTimer); } catch (e) {}

      var parsed = normalizeDecision(json);

      if (parsed.country) detectedCountry = parsed.country;
      if (parsed.sessionId) setSessionId(parsed.sessionId, parsed.country);

      if (parsed.ignoreAnalytics === true || parsed.ignoreAnalytics === false) {
        writeDecisionCache(parsed.ignoreAnalytics, parsed.outcome, parsed.country);
      }

      try {
        window.__REPUTABLE__ = {
          sessionId: getSessionId(),
          outcome: parsed.outcome || null,
          country: parsed.country || null,
          ignoreAnalytics: parsed.ignoreAnalytics,
          raw: parsed.raw || null,
          canPersist: canPersistSession(),
          error: !!err,
          source: 'network'
        };
        dispatchDecision(window.__REPUTABLE__);
      } catch (e2) {}

      if (getSessionId()) startBehaviorTracking();

      // Skip analytics ONLY when ignoreAnalytics === true. Otherwise load.
      if (!analyticsFinalized) {
        finalizeAnalytics(!(parsed.ignoreAnalytics === true));
      }
    });
  }
})();
</script>
<!-- End Reputation gate + conditional GTM loader -->

