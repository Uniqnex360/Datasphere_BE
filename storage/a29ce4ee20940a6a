<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
  $(document).on('click','.custom_delivery_button',function(e){
    e.preventDefault();
   $(this).closest('.section-product-template').find('.popup-overlay').fadeIn('slow');
    //console.log($(this).closest('.section-product-template').find('.popup-overlay').html());
    //alert('welcome');
    $('body').addClass('custom_active');
  }); 
  $('.pop-close').click(function(){
    $('.popup-overlay').fadeOut('slow');
    $('body').removeClass('custom_active');
  });
  $(document).on('click','.popup-overlay',function(){
    $(this).fadeOut('slow'); 
  });
  $( document ).on( 'keydown', function ( e ) {
    if (e.keyCode === 27) { 
      $('.popup-overlay').fadeOut('slow');
    }
  });
  $(document).on('click','.popup-body',function(e){
   // e.preventDefault();
    e.stopPropagation();
  });
</script>
