    <section class="product-details" ref="component">
        <div v-if="product.options && product.options.length > 1" class="product-options">
            <div class="container">
                <div class="options">
                    <div class="option" v-bind:class="isSelected(option)" :key="option.id"
                         v-for="(option, index) in product.options"
                         @mouseenter="preview(option)"
                         @mouseleave="rollbackPreview()"
                         @click="selectOption(option)">
                        <img :src="option.cover.mobile.url">
                        <p class="option-title">{{option.title}}</p>
                        <p class="option-sku">({{option.sku}})</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="product-option-details">
            <div class="container">
                <div class="section" v-if="product">
                    <div class="columns" v-if="mediaGallery">
