                            <div class="carousel-slider" v-if="product" v-bind:class="isCarouselInitialized()">
                                <ul class="slider-for">
                                    <li v-for="(item, index) in mediaGallery.items" :key="index">
                                        <img :alt="item.image.desktop.title" :src="item.image.desktop.url"/>
                                    </li>
                                </ul>
