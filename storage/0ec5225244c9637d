    <script>
        function loadDeferredStyles() {
            var stylesheetLight = document.getElementById('stylesheetLight');
            var stylesheetDark = document.getElementById('stylesheetDark');
            var stylesLoadedEvent = new CustomEvent('stylesLoaded', {
                bubbles: true,
                cancelable: true
            });

            function dispatchStylesLoaded() {
                document.dispatchEvent(stylesLoadedEvent);
            }

            if (window.matchMedia('(prefers-color-scheme: dark)').media === 'not all') {
                var fallbackLink = document.createElement('link');
                fallbackLink.rel = 'stylesheet';
                fallbackLink.href = 'https://www.updatestar.com/css/bundle.min.css?v=a492aaacadab95439e8d4b679de21706';
                fallbackLink.onload = dispatchStylesLoaded;
                document.head.appendChild(fallbackLink);
            } else {
                if (window.matchMedia('(prefers-color-scheme: dark)').matches) {
                    stylesheetDark.disabled = false;
                    if (stylesheetDark.sheet) {
                        dispatchStylesLoaded();
                    } else {
                        stylesheetDark.onload = dispatchStylesLoaded;
                    }
                } else {
                    stylesheetLight.disabled = false;
                    if (stylesheetLight.sheet) {
                        dispatchStylesLoaded();
                    } else {
                        stylesheetLight.onload = dispatchStylesLoaded;
                    }
                }
            }
        }
        window.addEventListener('load', loadDeferredStyles);
        function loadAds() {
            window.addEventListener('load', function () {
                var script = document.createElement('script');
                script.async = true;
                script.src = "https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js?client=ca-pub-4118765744455397";
                script.crossOrigin = "anonymous";
                document.body.appendChild(script);
            });
        }
    </script>
    
