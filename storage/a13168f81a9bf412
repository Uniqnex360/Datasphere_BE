function getReferer() {
    var reg = new RegExp("(^|&)referer=([^&]*)(&|$)");
    var r = window.location.search.replace("referer=/areyouahuman?","").substr(1).match(reg);
    var referer = window.location.origin;
    if (r) {
        referer = unescape(r[2]);
    }
    if (referer.indexOf('?') > 0) {
        referer += '&';
    } else {
        referer += '?';
    }
    referer += 'recaptcha=pass';

    return referer;
}

